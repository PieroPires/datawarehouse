      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="[MySQL - SugarCRM].[SqlCommand]">"SELECT DISTINCT A.id AS ID_REUNIAO,
	   D.user_name AS Usuario_Participante,
	   B.user_name AS Usuario_Criacao,
       B1.user_name AS Usuario_Responsavel,
       CASE WHEN A.assigned_user_id = C.user_id THEN \"S\" ELSE \"N\" END AS Flag_Responsavel_Reuniao,
	   E.id AS ContaID,
       E.name AS Conta,
       A.name AS Assunto,
       A.date_start AS Data_Inicio,
	   A.date_entered AS Data_Criacao,
       A.status,
       A.parent_type,
       A.duration_hours AS Duracao_Horas,
       A.duration_minutes AS Duracao_Minutos ,G.name AS Oportunidade 
FROM meetings A 
INNER JOIN users B ON B.id = A.created_by
				  AND B.deleted = 0 
INNER JOIN users B1 ON B1.id = A.assigned_user_id
				  AND B1.deleted = 0                   
INNER JOIN meetings_users C ON C.meeting_id = A.id
				  AND C.deleted = 0 
INNER JOIN users D ON D.id = C.user_id
				  AND D.deleted = 0
 LEFT OUTER JOIN accounts E ON E.id = A.parent_id
				  AND E.deleted = 0
                  AND A.parent_type = \"Accounts\"  
LEFT OUTER JOIN accounts_cstm F ON F.id_c = E.id 
LEFT OUTER JOIN opportunities G ON G.id = A.parent_id
								AND G.deleted = 0 
								AND A.parent_type = \"Opportunities\" WHERE A.deleted = 0 AND A.date_entered &gt; " + @[User::DATA_ALTERACAO]</DTS:PropertyExpression>
