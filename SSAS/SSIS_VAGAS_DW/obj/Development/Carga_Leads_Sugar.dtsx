      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="[MySQL].[SqlCommand]">"SELECT CONVERT(A.id  USING Latin1) AS ID_LEAD,
	   CONVERT(A.first_name USING Latin1) AS PRIMEIRO_NOME,
       CONVERT(A.last_name USING Latin1) AS ULTIMO_NOME,
       CONVERT(A.account_name USING Latin1) AS CONTA,
      CASE WHEN A.lead_source IS NULL OR A.lead_source = \"\" 
			THEN CONVERT(CONVERT(B.form_origem_c,CHAR(50)) USING Latin1) 
            ELSE CONVERT(CONVERT(A.lead_source,CHAR(50)) USING Latin1) END AS ORIGEM,
       CONVERT(A.converted USING Latin1) AS FLAG_CONVERTIDO,
       CONVERT(B.industry_c USING Latin1) AS SEGMENTO,
       CONVERT(C.user_name USING Latin1) AS USUARIO,
       CONVERT(A.status USING Latin1) AS STATUS,
       A.date_entered AS DATA_INCLUSAO,
       A.date_modified AS DATA_ALTERACAO,
       CONVERT(E.email_address USING Latin1) AS EMAIL,
       CONVERT(B.como_conheceu_o_vagas_c USING Latin1) AS COMO_CONHECEU_VAGAS,
       CONVERT(B.motivo_da_perda_c USING Latin1) AS MOTIVO_PERDA,
       CONVERT(F.name USING Latin1) AS OPORTUNIDADE,
       CONVERT(F.sales_stage USING Latin1) AS FASE_OPORTUNIDADE,
       CONVERT(F.amount USING Latin1) AS VALOR_VENDA,CONVERT(A.opportunity_id USING Latin1) AS OPORTUNIDADE_ID,
       CASE WHEN A.description LIKE \"%Créditos VAGAS%\" THEN 1 ELSE 0 END AS CREDITOS_VAGAS, (SELECT	CONVERT(A1.name USING Latin1) AS CAMPANHA
        FROM	sugarcrm.campaigns AS A1
		WHERE	A.campaign_id = A1.id) AS CAMPANHA ,
	   CONVERT(A.account_id USING Latin1) AS CONTA_ID ,
	   B.data_de_conclusao_c AS DATA_CONCLUSAO ,
       CONVERT(A.title USING Latin1) AS CARGO ,
       CONVERT(B.form_origem_c USING Latin1) AS FORMULARIO_ORIGEM ,
       CONVERT(B.lead_type_c USING Latin1) AS CATEGORIA_LEAD
 FROM leads A
 LEFT OUTER JOIN leads_cstm B ON B.id_c = A.id
 INNER JOIN users C ON C.id = A.assigned_user_id 
				   AND C.deleted = 0
LEFT OUTER JOIN email_addr_bean_rel D ON D.bean_id = A.id
									  AND D.deleted = 0
                                      AND D.primary_address = '1'
                                      AND D.bean_module = 'Leads' 
LEFT OUTER JOIN email_addresses E ON E.id = D.email_address_id
								  AND E.deleted = 0 
LEFT OUTER JOIN opportunities F ON F.id = A.opportunity_id 
								AND F.deleted = 0
 LEFT OUTER JOIN opportunities_cstm G ON G.id_c = F.id 
WHERE A.deleted = 0 

 AND A.date_modified &gt; " +  @[User::DATA_ALTERACAO]</DTS:PropertyExpression>
