      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="SqlStatementSource">"DROP TABLE IF EXISTS VAGAS_DW.TMP_FINANCEIRO_SENSEDATA
GO

SELECT 
	A.COD_CLI_CRM AS IdConta_CRM ,
	A.NFE AS NFE ,
	A.PRODUTO  AS Produto ,
	A.PARCELA AS Parcela ,
	A.LINK_NFE AS Link_NFE ,
	CONVERT(DATE, A.DATA_VENCIMENTO_REAL) AS Data_Vencimento_Real ,
	IIF(CONVERT(VARCHAR(10), CONVERT(DATE, A.DATA_BAIXA_PAGTO)) = '1900-01-01', '', CONVERT(VARCHAR(10), CONVERT(DATE, A.DATA_BAIXA_PAGTO))) AS Data_Pagamento ,
	CONVERT(DATE, A.DATA_EMISSAO) AS Data_emissao ,
	A.VALOR AS Valor_Original ,
	A.FAIXA_ATRASO AS Faixa_atraso ,
	A.STATUS_ATRASO AS Status_atraso ,
	IIF(A.AVISO_PREVIO = 1, 'Sim', 'Não') AS Aviso_Previo ,
	A.STATUS_INADIMPLENCIA AS Status_Inadimplencia
INTO VAGAS_DW.TMP_FINANCEIRO_SENSEDATA		
FROM [VAGAS_DW].[FATURAS] AS A
WHERE
	A.DATA_REFERENCIA = (SELECT MAX(A1.DATA_REFERENCIA)
							FROM  [VAGAS_DW].[FATURAS] AS A1)
	AND A.TIPO_ORIGEM = 'NF'
	AND A.COD_CLI_CRM IN (SELECT ID_CLIENTECRM FROM VAGAS_DW.TMP_CLIENTES_SENSEDATA)
	AND A.DATA_EMISSAO &gt; '" + @[User::DATA_CARGA_INICIO] + "'
GO"</DTS:PropertyExpression>
      <DTS:ObjectData>
