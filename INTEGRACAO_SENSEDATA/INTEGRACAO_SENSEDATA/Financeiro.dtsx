                  description="O comando SQL a ser executado."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT	
	A.CONTA_ID AS IdConta_CRM ,
	A.NUMERO_CASO AS Ticket ,
	A.EMAIL_CONTATO AS Contato ,
	A.USUARIO_RESPONSAVEL AS Responsavel ,
	B.Assunto AS Assunto ,
	A.MOTIVO_CONTATO AS Motivo_contato ,
	A.ORIGEM_SUPORTE AS Origem ,
	NULL AS Categoria ,
	CONCAT(A.TIPO, ' --&gt; ' ,A.SUB_TIPO) AS Classificacao ,
	CONVERT(DATE, A.DATA_INCLUSAO_NORMALIZADA) AS Data_abertura ,
	CONVERT(DATE, A.DATA_FECHAMENTO) AS Data_conclusao ,
	NULL AS Data_SLA ,
	CAST(A.STATUS AS VARCHAR(100)) AS [Status] ,
	A.CRITICIDADE AS Criticidade ,
	CONCAT('https://crm.vagas.com.br/index.php?module=Cases&amp;action=DetailView&amp;record=', A.ID_CASO) AS Link_ticket
FROM	
	[VAGAS_DW].[CASOS] AS A	
	LEFT OUTER JOIN TMP_Tab_casos AS B ON A.ID_CASO = B.Id COLLATE SQL_Latin1_General_CP1_CI_AI
WHERE
	A.FILA_ATENDIMENTO = 'suporte_empresas'
	AND A.[STATUS] IN ('fechado', 'fechado_sem_resposta')
	AND A.DATA_FECHAMENTO &gt;= DATEADD(MONTH, -18, CONVERT(DATE, GETDATE()))</property>
