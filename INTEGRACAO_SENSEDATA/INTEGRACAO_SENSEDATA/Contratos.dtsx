#x9;&#x9;&#x9;&#x9;'projeto','revisao_de_perfil','venda_pontual','consultoria_e_treinamento',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'rescisao','retencao')&#xA;&#x9;&#x9;&#x9;GROUP BY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;H.CONTA_ID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.CONTAID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CONVERT(DATE, A.DataFechamento) ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.OportunidadeID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.Oportunidade ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.Produto ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.PRODUTO_RECORRENTE ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.ValorProdutoFINAL ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;IIF(A.Fase = 'fechado_e_ganho', 'FECHADA', 'ABERTA') ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CONCAT_WS(' / ', F.MOTIVO_FECHAMENTO, F.MOTIVO_FECHAMENTO_DETALHE) ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.MODELO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.PRAZO_CONTRATADO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.DATA_INICIO_USO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.DATA_FIM_USO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.RENOVACAO_AUTOMATICA ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;Plano_Conta.PLANO,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CASE&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN H.ID_PROPOSTA_APROVADA IS NOT NULL AND A.PRODUTO_RECORRENCIA IN ('monthly', 'annually')&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN A.ValorProdutoFINAL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;I.COD_CLI) AS SUBQUERY ;&#xA;&#xA;&#xA;&#xA;-- Carga do campo Data_cancelamento:&#xA;drop table if exists  #Tab_Data_cancelamento ;&#xA;SELECT&#x9;DISTINCT A.OportunidadeID,&#xA;&#x9;&#x9;&#x9;&#x9; A.PropostaID,&#xA;&#x9;&#x9;&#x9;&#x9; B.DATA_FIM_USO AS Data_cancelamento&#xA;INTO&#x9;#Tab_Data_cancelamento&#xA;FROM&#x9;[VAGAS_DW].[VAGAS_DW].[OPORTUNIDADES] AS A&#x9;&#x9;LEFT OUTER JOIN OPENQUERY (MYSQLPROVIDER, 'SELECT A1.id_c AS ID_PROPOSTA ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  A1.data_fim_uso_c AS DATA_FIM_USO&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;   FROM&#x9;  sugarcrm.oqc_offering_cstm AS A1') AS B&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON A.PropostaID = B.ID_PROPOSTA COLLATE SQL_Latin1_General_CP1_CI_AI&#xA;WHERE&#x9;(EXISTS  (SELECT&#x9;*&#xA;&#x9;&#x9;&#x9;&#x9;  FROM&#x9;[STAGE].[VAGAS_DW].[TMP_CLIENTES_SENSEDATA] AS A1&#xA;&#x9;&#x9;&#x9;&#x9;  WHERE&#x9;A.CONTAID = A1.Id_ClienteCRM&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.TIPO_CONTA = 'cliente_em_aviso_previo'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.CATEGORIA_CONTA = 'cliente_em_aviso_previo')&#xA;&#x9;&#x9;&#x9;OR EXISTS (SELECT *&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;   FROM&#x9; [STAGE].[VAGAS_DW].[SNAPSHOT_CLIENTES_SENSEDATA_STATUS] AS A1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;   WHERE A.CONTAID = A1.CONTA_ID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; AND LEFT(A1.STATUS_CONTA_ID, 7) = 'INATIVO'))&#xA;&#x9;&#x9;&#x9;AND A.DataFechamento = (SELECT MAX(A1.DataFechamento)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM  [VAGAS_DW].[VAGAS_DW].[OPORTUNIDADES] AS A1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE A.CONTAID = A1.CONTAID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  AND A1.Fase = 'fechado_e_ganho'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  AND A1.INSERT_MANUAL IS NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  AND A1.PRODUTO_RECORRENCIA IN ('monthly', 'annually'))&#xA;&#x9;&#x9;&#x9;AND A.Fase = 'fechado_e_ganho'&#xA;&#x9;&#x9;&#x9;AND A.OportunidadeCategoria = 'rescisão'&#xA;&#x9;&#x9;&#x9;AND A.DataCriacao = (SELECT MAX(A1.DataCriacao)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; FROM&#x9;[VAGAS_DW].[VAGAS_DW].[OPORTUNIDADES] AS A1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; WHERE&#x9;A.CONTAID = A1.CONTAID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.Fase = 'fechado_e_ganho'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.OportunidadeCategoria = 'rescisão'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.INSERT_MANUAL IS NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.PRODUTO_RECORRENCIA IN ('monthly', 'annually'))&#xA;&#x9;&#x9;&#x9;AND A.INSERT_MANUAL IS NULL&#xA;&#x9;&#x9;&#x9;AND A.PRODUTO_RECORRENCIA IN ('monthly', 'annually') ;&#xA;&#xA;&#xA;-- Atualização na tabela com a data_fim_uso:&#xA;UPDATE [STAGE].[VAGAS_DW].[TMP_CONTRATOS_SENSEDATA]&#xA;SET&#x9;   Data_cancelamento = B.Data_cancelamento&#xA;FROM   [STAGE].[VAGAS_DW].[TMP_CONTRATOS_SENSEDATA] AS A&#x9;&#x9;INNER JOIN #Tab_Data_cancelamento AS B&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON A.ID_OPORTUNIDADE = B.OportunidadeID ;&#xA;&#xA;&#xA;&#xA;&#xA;-- Carrega as contas que são Pai com o respectivo plano:&#xA;drop table if exists stage.sensedata.plano_conta_owner ;&#xA;&#xA;select&#x9;distinct contrat_sdt.conta_id,&#xA;&#x9;&#x9;contrat_sdt.plano&#xA;into&#x9;stage.sensedata.plano_conta_owner&#xA;from&#x9;stage.vagas_dw.tmp_contratos_sensedata as contrat_sdt&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;where&#x9;exists (select *&#xA;&#x9;&#x9;&#x9;&#x9;from&#x9;stage.vagas_dw.snapshot_clientes_sensedata_status as cli_sdt_sts&#xA;&#x9;&#x9;&#x9;&#x9;where&#x9;contrat_sdt.conta_id = cli_sdt_sts.conta_id&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and cli_sdt_sts.hierarquia_conta = 'Conta Pai') ;&#xA;&#xA;&#xA;-- Atualiza o campo Plano das contas membro &#xA;update&#x9;stage.vagas_dw.tmp_contratos_sensedata&#xA;set&#x9;&#x9;plano = plan_cnt_owner.plano&#xA;from&#x9;stage.vagas_dw.tmp_contratos_sensedata as contrat_sdt_membro&#x9;&#x9;inner join (select&#x9;cnt_membro.conta_id,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;cnt_membro.conta_id_pai&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from&#x9;vagas_dw.vagas_dw.contas_membro_crm as cnt_membro) as cnt_membro&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;on contrat_sdt_membro.conta_id = cnt_membro.conta_id&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;inner join stage.sensedata.plano_conta_owner as plan_cnt_owner&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;on cnt_membro.conta_id_pai = plan_cnt_owner.conta_id&#xA;where&#x9;exists (select *&#xA;&#x9;&#x9;&#x9;&#x9;from&#x9;stage.vagas_dw.snapshot_clientes_sensedata_status as cli_sdt_sts_membro&#xA;&#x9;&#x9;&#x9;&#x9;where&#x9;contrat_sdt_membro.conta_id = cli_sdt_sts_membro.conta_id&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and cli_sdt_sts_membro.hierarquia_conta = 'Conta Filha')&#xA;&#x9;&#x9;and isnull(contrat_sdt_membro.plano, '') = '' ;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
