      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2017 RC1; © 2017 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{01D8D567-93B2-4E49-A51A-28782164D7C4}"
          SQLTask:SqlStatementSource="DROP TABLE IF EXISTS #Tab_Opp_Rescisao_Motivos ;&#xA;&#xA;SELECT&#x9;A.CONTAID ,&#xA;&#x9;&#x9;A.OportunidadeID ,&#xA;&#x9;&#x9;ISNULL(B.Concorrente_Outro, '') AS Concorrente_outro ,&#xA;&#x9;&#x9;ISNULL(B.Outra_empresa_cliente, '') AS Outra_empresa_cliente&#xA;INTO&#x9;#Tab_Opp_Rescisao_Motivos&#xA;FROM&#x9;[VAGAS_DW].[OPORTUNIDADES] AS A&#x9;&#x9;INNER JOIN OPENQUERY(MYSQLPROVIDER, 'SELECT&#x9;A1.id_c AS OportunidadeID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.concorrente_outro_c AS Concorrente_Outro ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.outra_empresa_cliente_c AS Outra_empresa_cliente&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; FROM&#x9;sugarcrm.opportunities_cstm AS A1') AS B&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON A.OportunidadeID = B.OportunidadeID COLLATE SQL_Latin1_General_CP1_CI_AI&#xA;WHERE&#x9;EXISTS ( SELECT&#x9;1&#xA;&#x9;&#x9;&#x9;&#x9; FROM&#x9;[VAGAS_DW].[CLIENTES] AS A1&#xA;&#x9;&#x9;&#x9;&#x9; WHERE&#x9;A.CONTAID = A1.CONTA_ID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.EX_CLIENTE = 0)&#xA;&#x9;&#x9;AND (A.OportunidadeCategoria = 'rescisão' AND A.Fase = 'fechado_e_ganho'&#xA;&#x9;&#x9;&#x9; OR A.OportunidadeCategoria = 'retencao' AND A.Fase = 'fechado_e_perdido' AND A.DataFechamento &gt;= '20210101') &#xA;&#x9;&#x9;AND A.RECORRENTE = 1 ;&#xA;&#xA;CREATE NONCLUSTERED INDEX IDX_#Tab_Opp_Rescisao_Motivos_OportunidadeID ON #Tab_Opp_Rescisao_Motivos (OportunidadeID) ;&#xA;&#xA;&#xA;DROP TABLE IF EXISTS #TMP_DADOS_PROPOSTA ;&#xA;&#xA;SELECT&#x9;*&#xA;INTO&#x9;#TMP_DADOS_PROPOSTA&#xA;FROM&#x9;OPENQUERY(MYSQLPROVIDER, 'SELECT A.id_c AS ID_PROPOSTA ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; A.data_inicio_uso_c AS DATA_INICIO_USO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x