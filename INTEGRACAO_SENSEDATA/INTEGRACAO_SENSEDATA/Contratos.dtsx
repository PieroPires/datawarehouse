      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2017 RC1; © 2017 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{01D8D567-93B2-4E49-A51A-28782164D7C4}"
          SQLTask:SqlStatementSource="DROP TABLE IF EXISTS #Tab_Opp_Rescisao_Motivos ;&#xA;&#xA;SELECT&#x9;A.CONTAID ,&#xA;&#x9;&#x9;A.OportunidadeID ,&#xA;&#x9;&#x9;ISNULL(B.Concorrente_Outro, '') AS Concorrente_outro ,&#xA;&#x9;&#x9;ISNULL(B.Outra_empresa_cliente, '') AS Outra_empresa_cliente&#xA;INTO&#x9;#Tab_Opp_Rescisao_Motivos&#xA;FROM&#x9;[VAGAS_DW].[OPORTUNIDADES] AS A&#x9;&#x9;INNER JOIN OPENQUERY(MYSQLPROVIDER, 'SELECT&#x9;A1.id_c AS OportunidadeID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.concorrente_outro_c AS Concorrente_Outro ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.outra_empresa_cliente_c AS Outra_empresa_cliente&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; FROM&#x9;sugarcrm.opportunities_cstm AS A1') AS B&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON A.OportunidadeID = B.OportunidadeID COLLATE SQL_Latin1_General_CP1_CI_AI&#xA;WHERE&#x9;EXISTS ( SELECT&#x9;1&#xA;&#x9;&#x9;&#x9;&#x9; FROM&#x9;[VAGAS_DW].[CLIENTES] AS A1&#xA;&#x9;&#x9;&#x9;&#x9; WHERE&#x9;A.CONTAID = A1.CONTA_ID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.EX_CLIENTE = 0)&#xA;&#x9;&#x9;AND A.OportunidadeCategoria = 'rescisão'&#xA;&#x9;&#x9;AND A.Fase = 'fechado_e_ganho' &#xA;&#x9;&#x9;AND A.RECORRENTE = 1 ;&#xA;&#xA;CREATE NONCLUSTERED INDEX IDX_#Tab_Opp_Rescisao_Motivos_OportunidadeID ON #Tab_Opp_Rescisao_Motivos (OportunidadeID) ;&#xA;&#xA;&#xA;DROP TABLE IF EXISTS #TMP_DADOS_PROPOSTA ;&#xA;&#xA;SELECT&#x9;*&#xA;INTO&#x9;#TMP_DADOS_PROPOSTA&#xA;FROM&#x9;OPENQUERY(MYSQLPROVIDER, 'SELECT A.id_c AS ID_PROPOSTA ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; A.data_inicio_uso_c AS DATA_INICIO_USO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; A.data_fim_uso_c AS DATA_FIM_USO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; A.prazo_c AS PRAZO_CONTRATADO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; IF(A.renovacao_automatica_c = 1, ''SIM'', ''NÃO'') AS RENOVACAO_AUTOMATICA ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; CASE A.forma_de_pagamento_c &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; &#x9;WHEN ''mensal''&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; &#x9;&#x9;THEN ''MRR''&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN ''semestral''&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN ''HRR''&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN ''anual''&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN ''ARR''&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE UPPER(A.forma_de_pagamento_c)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END AS MODELO            &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM    sugarcrm.oqc_offering_cstm AS A') AS SUBQUERY ;&#xA;&#xA;&#xA;TRUNCATE TABLE VAGAS_DW.TMP_CONTRATOS_SENSEDATA&#xA;&#xA;INSERT INTO VAGAS_DW.TMP_CONTRATOS_SENSEDATA&#xA;SELECT&#x9;SUBQUERY.CONTA_ID ,&#xA;&#x9;&#x9;SUBQUERY.DATA_OPORTUNIDADE ,&#xA;&#x9;&#x9;SUBQUERY.ID_OPORTUNIDADE ,&#xA;&#x9;&#x9;SUBQUERY.OPORTUNIDADE ,&#xA;&#x9;&#x9;SUBQUERY.ITEM ,&#xA;&#x9;&#x9;SUBQUERY.DESCRICAO ,&#xA;&#x9;&#x9;SUBQUERY.MODELO ,&#xA;&#x9;&#x9;SUBQUERY.TIPO ,&#xA;&#x9;&#x9;SUBQUERY.PARCELAS ,&#xA;&#x9;&#x9;SUBQUERY.DATA_INICIO_USO ,&#xA;&#x9;&#x9;SUBQUERY.DATA_FIM_USO ,&#xA;&#x9;&#x9;SUBQUERY.VALOR_ITEM ,&#xA;&#x9;&#x9;SUBQUERY.STATUS_OPORTUNIDADE ,&#xA;&#x9;&#x9;SUBQUERY.RENOVACAO_AUTOMATICA ,&#xA;&#x9;&#x9;CONCAT_WS(' / ', CASE&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN ISNULL(SUBQUERY.DATAFECHAMENTO_FIT, '1900-01-01') &gt; ISNULL(SUBQUERY.DATAFECHAMENTO_RECRUTADOR, '1900-01-01')&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN 'FIT'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN ISNULL(SUBQUERY.DATAFECHAMENTO_RECRUTADOR, '1900-01-01') &gt; ISNULL(SUBQUERY.DATAFECHAMENTO_FIT, '1900-01-01')&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN 'RECRUTADOR'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN ISNULL(SUBQUERY.DATAFECHAMENTO_RECRUTADOR, '1900-01-01') = ISNULL(SUBQUERY.DATAFECHAMENTO_FIT, '1900-01-01')&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; AND SUBQUERY.DATAFECHAMENTO_FIT IS NOT NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN 'FIT/RECRUTADOR'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN SUBQUERY.DATAFECHAMENTO_FIT IS NULL AND SUBQUERY.DATAFECHAMENTO_RECRUTADOR IS NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END , SUBQUERY.REDES) AS PLANO ,&#xA;&#x9;&#x9;SUBQUERY.USUARIOS_CONTRATADOS ,&#xA;&#x9;&#x9;SUBQUERY.DATA_CANCELAMENTO ,&#xA;&#x9;&#x9;SUBQUERY.MOTIVO_CANCELAMENTO ,&#xA;&#x9;&#x9;SUBQUERY.PERIODICIDADE ,&#xA;&#x9;&#x9;SUBQUERY.VALOR_MRR&#xA;FROM&#x9;(&#xA;&#x9;&#x9;&#x9;SELECT&#x9;A.CONTAID AS CONTA_ID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CONVERT(DATE, A.DataFechamento) AS DATA_OPORTUNIDADE ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.OportunidadeID AS ID_OPORTUNIDADE ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.Oportunidade AS OPORTUNIDADE ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.Produto AS ITEM ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.PRODUTO_RECORRENTE AS DESCRICAO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.MODELO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;STRING_AGG(B.Produto, ' / ') AS TIPO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.PRAZO_CONTRATADO AS PARCELAS ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.DATA_INICIO_USO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.DATA_FIM_USO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.ValorProdutoFINAL AS VALOR_ITEM ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;IIF(A.Fase = 'fechado_e_ganho', 'FECHADA', 'ABERTA') AS STATUS_OPORTUNIDADE ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.RENOVACAO_AUTOMATICA ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT&#x9;TOP 1 'FIT'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9; FROM&#x9;[VAGAS_DW].[OPORTUNIDADES] AS A1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9; WHERE&#x9;A.CONTAID = A1.CONTAID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND 