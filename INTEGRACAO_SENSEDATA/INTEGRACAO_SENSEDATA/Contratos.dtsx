xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.Fase = 'fechado_e_ganho'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.RECORRENTE = 1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.OportunidadeCategoria = 'rescisão'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.INSERT_MANUAL IS NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  ORDER BY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.DataFechamento DESC ) AS F&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT OUTER JOIN #TMP_DADOS_PROPOSTA AS G ON A.PropostaID = G.ID_PROPOSTA COLLATE SQL_Latin1_General_CP1_CI_AI&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT OUTER JOIN OPENQUERY(MYSQLPROVIDER, 'SELECT&#x9;A1.id_c AS CONTA_ID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.oqc_offering_id_c AS ID_PROPOSTA_APROVADA&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  FROM&#x9;&#x9;sugarcrm.accounts_cstm AS A1') AS H&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON A.CONTAID = H.CONTA_ID COLLATE SQL_Latin1_General_CP1_CI_AI&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A.PropostaID = H.ID_PROPOSTA_APROVADA COLLATE SQL_Latin1_General_CP1_CI_AI&#xA;&#x9;&#x9;&#x9;WHERE&#x9;A.Fase IN ('avaliacao_interna','contrato','fechado_e_ganho','projeto_suspenso','proposta_comercial','prospeccao') &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;AND A.INSERT_MANUAL IS NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;AND A.OportunidadeCategoria IN ('cliente_potencial','cliente_cotacao','renovacao','parceiro',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'projeto','revisao_de_perfil','venda_pontual','consultoria_e_treinamento',&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'rescisao','retencao')&#xA;&#x9;&#x9;&#x9;GROUP BY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.CONTAID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CONVERT(DATE, A.DataFechamento) ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.OportunidadeID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.Oportunidade ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.Produto ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.PRODUTO_RECORRENTE ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;A.ValorProdutoFINAL ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;IIF(A.Fase = 'fechado_e_ganho', 'FECHADA', 'ABERTA') ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CONCAT_WS(' / ', F.MOTIVO_FECHAMENTO, F.MOTIVO_FECHAMENTO_DETALHE) ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.MODELO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.PRAZO_CONTRATADO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.DATA_INICIO_USO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.DATA_FIM_USO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;G.RENOVACAO_AUTOMATICA ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CASE&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN H.ID_PROPOSTA_APROVADA IS NOT NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; AND A.RECORRENTE = 1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;THEN A.ValorProdutoFINAL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE NULL&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;END) AS SUBQUERY" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
