                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="O comando SQL a ser executado."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT	A.CONTA_ID AS Id_ClienteCRM ,
		B.COD_CONTATO AS IdContato_CRM ,
		B.Nome ,
		B.Email ,
		B.Cargo ,
		B.TELEFONE_CELULAR AS Tel1 ,
		B.TELEFONE_CONTATO AS Tel2 ,
		NULL AS Perfil_Linkedin ,
		CASE
			WHEN ISNULL(B.RUA, '') &lt;&gt; '' AND ISNULL(B.CIDADE, '') &lt;&gt; '' AND ISNULL(B.ESTADO, '') &lt;&gt; '' AND ISNULL(B.PAIS, '') &lt;&gt; ''
				THEN REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(B.RUA, '.', ''), ',', ''), '?', ''), ':', '') + ', ' + 
								REPLACE(REPLACE(REPLACE(REPLACE(B.CIDADE, '.', ''), ',', ''), '?', ''), ':', '') + ' - ' + 
								REPLACE(REPLACE(REPLACE(REPLACE(B.ESTADO, '.', ''), ',', ''), '?', ''), ':', '') + ', ' + 
								REPLACE(REPLACE(REPLACE(REPLACE(B.PAIS, '.', ''), ',', ''), '?', ''), ':', ''), ',,', '')
			ELSE NULL
		END	AS Endereco ,
		IIF(B.[ADMIN] = 1, 'Administrador', 'Recrutador') AS Tipo_contato ,
		FORMAT(B.DATA_INCLUSAO, 'dd/MM/yyyy') AS Data_cadastro ,
		FORMAT(B.DATA_MODIFICACAO, 'dd/MM/yyyy') AS Data_UltAtualizacao ,
		B.DtUltAtividade AS Data_UltAtividade_CRM ,
		IIF(C.IdContato_CRM IS NULL, 'Não', 'Sim') AS Main_Sponsor
FROM	[VAGAS_DW].[CONTAS_CRM] AS A		INNER JOIN [VAGAS_DW].[CONTATOS_CRM] AS B ON A.CONTA_ID = B.COD_CONTA_CRM
											LEFT OUTER JOIN TMP_Tab_MainSponsor AS C ON B.COD_CONTATO = C.IdContato_CRM COLLATE SQL_Latin1_General_CP1_CI_AI
WHERE	B.FUNCIONARIO_DESLIGADO = 'NÃO'
		AND B.COD_CONTA_CRM IN (SELECT ID_CLIENTECRM FROM VAGAS_DW.TMP_CLIENTES_SENSEDATA)
UNION ALL
SELECT	A.CONTA_ID AS Id_ClienteCRM ,
		B.COD_CONTATO AS IdContato_CRM ,
		B.Nome ,
		B.Email ,
		B.Cargo ,
		B.TELEFONE_CELULAR AS Tel1 ,
		B.TELEFONE_CONTATO AS Tel2 ,
		NULL AS Perfil_Linkedin ,
		CASE
			WHEN ISNULL(B.RUA, '') &lt;&gt; '' AND ISNULL(B.CIDADE, '') &lt;&gt; '' AND ISNULL(B.ESTADO, '') &lt;&gt; '' AND ISNULL(B.PAIS, '') &lt;&gt; ''
				THEN REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(B.RUA, '.', ''), ',', ''), '?', ''), ':', '') + ', ' + 
								REPLACE(REPLACE(REPLACE(REPLACE(B.CIDADE, '.', ''), ',', ''), '?', ''), ':', '') + ' - ' + 
								REPLACE(REPLACE(REPLACE(REPLACE(B.ESTADO, '.', ''), ',', ''), '?', ''), ':', '') + ', ' + 
								REPLACE(REPLACE(REPLACE(REPLACE(B.PAIS, '.', ''), ',', ''), '?', ''), ':', ''), ',,', '')
			ELSE NULL
		END	AS Endereco ,
		IIF(B.[ADMIN] = 1, 'Administrador', 'Recrutador') AS Tipo_contato ,
		FORMAT(B.DATA_INCLUSAO, 'dd/MM/yyyy') AS Data_cadastro ,
		FORMAT(B.DATA_MODIFICACAO, 'dd/MM/yyyy') AS Data_UltAtualizacao ,
		B.DtUltAtividade AS Data_UltAtividade_CRM ,
		IIF(C.IdContato_CRM IS NULL, 'Não', 'Sim') AS Main_Sponsor
FROM	[VAGAS_DW].[CONTAS_MEMBRO_CRM] AS A		INNER JOIN [VAGAS_DW].[CONTATOS_CRM] AS B ON A.CONTA_ID = B.COD_CONTA_CRM
												LEFT OUTER JOIN TMP_Tab_MainSponsor AS C ON B.COD_CONTATO = C.IdContato_CRM COLLATE SQL_Latin1_General_CP1_CI_AI
WHERE	B.FUNCIONARIO_DESLIGADO = 'NÃO' 
		AND B.COD_CONTA_CRM IN (SELECT ID_CLIENTECRM FROM VAGAS_DW.TMP_CLIENTES_SENSEDATA);</property>
