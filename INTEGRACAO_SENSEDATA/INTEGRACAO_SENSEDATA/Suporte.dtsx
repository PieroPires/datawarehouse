          SQLTask:Connection="{01D8D567-93B2-4E49-A51A-28782164D7C4}"
          SQLTask:SqlStatementSource="DROP TABLE IF EXISTS TMP_Tab_casos&#xA;GO&#xA;&#xA;SELECT&#x9;*&#xA;INTO&#x9;TMP_Tab_casos&#xA;FROM&#x9;OPENQUERY(MYSQLPROVIDER, 'SELECT A.id AS Id , A.name AS Assunto&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;FROM cases A LEFT OUTER JOIN cases_cstm B ON B.id_c = A.id&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE B.data_fechamento_c &gt; ''2018-03-09'' ') AS A&#xA;GO&#xA;&#xA;CREATE NONCLUSTERED INDEX IDX_TMP_Tab_casos_Id ON TMP_Tab_casos (Id)&#xA;GO&#xA;&#xA;DROP TABLE IF EXISTS VAGAS_DW.TMP_CASOS_SENSEDATA&#xA;GO&#xA;&#xA;SELECT&#x9;&#xA;&#x9;A.CONTA_ID AS IdConta_CRM ,&#xA;&#x9;A.NUMERO_CASO AS Ticket ,&#xA;&#x9;A.EMAIL_CONTATO AS Contato ,&#xA;&#x9;A.USUARIO_RESPONSAVEL AS Responsavel ,&#xA;&#x9;B.Assunto AS Assunto ,&#xA;&#x9;A.MOTIVO_CONTATO AS Motivo_contato ,&#xA;&#x9;A.ORIGEM_SUPORTE AS Origem ,&#xA;&#x9;NULL AS Categoria ,&#xA;&#x9;CONCAT(A.TIPO, ' --&gt; ' ,A.SUB_TIPO) AS Classificacao ,&#xA;&#x9;CONVERT(DATE, A.DATA_INCLUSAO_NORMALIZADA) AS Data_abertura ,&#xA;&#x9;CONVERT(DATE, A.DATA_FECHAMENTO) AS Data_conclusao ,&#xA;&#x9;NULL AS Data_SLA ,&#xA;&#x9;CAST(A.STATUS AS VARCHAR(100)) AS [Status] ,&#xA;&#x9;A.CRITICIDADE AS Criticidade ,&#xA;&#x9;CONCAT('https://crm.vagas.com.br/index.php?module=Cases&amp;action=DetailView&amp;record=', A.ID_CASO) AS Link_ticket&#xA;INTO VAGAS_DW.TMP_CASOS_SENSEDATA&#xA;FROM&#x9;&#xA;&#x9;[VAGAS_DW].[CASOS] AS A&#x9;&#xA;&#x9;LEFT OUTER JOIN TMP_Tab_casos AS B ON A.ID_CASO = B.Id COLLATE SQL_Latin1_General_CP1_CI_AI&#xA;WHERE&#xA;&#x9;A.FILA_ATENDIMENTO = 'suporte_empresas'&#xA;&#x9;AND A.[STATUS] IN ('fechado', 'fechado_sem_resposta')&#xA;&#x9;AND A.conta_id in (SELECT id_clientecrm FROM VAGAS_DW.TMP_CLIENTES_SENSEDATA)&#xA;&#x9;AND A.DATA_FECHAMENTO &gt; '2018-03-09'&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
