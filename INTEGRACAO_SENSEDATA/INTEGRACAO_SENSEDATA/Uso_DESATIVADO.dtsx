      <DTS:PropertyExpression
        DTS:Name="SqlStatementSource">"DROP TABLE IF EXISTS VAGAS_DW.TMP_USO_SENSEDATA
GO

SELECT *
INTO VAGAS_DW.TMP_USO_SENSEDATA
FROM (
	SELECT	B.IdContaCRM_cli COLLATE SQL_Latin1_General_CP1_CI_AI AS IdConta_CRM ,
			A.Cod_cli ,
			A.Cod_func ,
			'Avaliação e Seleção' AS Modulo ,
			A.Versao_Triagem AS Produto ,
			'Pesquisa --&gt;' + ' ' + A.Contexto AS Acao ,
			A.Data_Triagem AS [Data] ,
			COUNT(*) AS Qtd
	FROM	[VAGAS_DW].[VAGAS_DW].[TRIAGENS_VAGAS] AS A		INNER JOIN [hrh-data].[dbo].[Clientes] AS B ON A.Cod_cli = B.Cod_cli
	WHERE	A.DATA_TRIAGEM &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			B.IdContaCRM_cli ,
			A.Cod_cli ,
			A.Cod_func ,
			A.Versao_Triagem ,
			A.Contexto ,
			A.Data_Triagem 
	UNION ALL
	SELECT	A.IdConta_CRM ,
			A.Cod_cli ,
			A.Cod_func ,
			'Colaboração' AS Modulo ,
			'Requisição de Vagas' AS Produto ,
			'Abertura de requisição --&gt;' + ' ' + A.Status_requisicao AS Acao ,
			A.Data_requisicao ,
			COUNT(*) AS Qtd
	FROM	[VAGAS_DW].[Requisicao_VagasEpartner] AS A
	WHERE	A.DATA_REQUISICAO &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			A.IdConta_CRM ,
			A.Cod_cli ,
			A.Cod_func ,
			A.Status_requisicao,
			A.Data_requisicao

	UNION ALL
	SELECT	A.IdConta_CRM ,
			A.Cod_cli ,
			A.Cod_func ,
			'Acesso' AS Modulo ,
			'VAGAS e-partner' AS Produto ,
			A.Tipo AS Acao ,
			A.Data_Login ,
			COUNT(*) AS Qtd
	FROM	[VAGAS_DW].[VAGAS_DW].[Login_VagasEpartner_Clientes] AS A
	WHERE	A.Data_login &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			A.IdConta_CRM ,
			A.Cod_cli ,
			A.Cod_func ,
			A.Tipo ,
			A.Data_Login
	UNION ALL
	SELECT	B.IdContaCRM_cli AS IdConta_CRM ,
			A.Cod_cli ,
			C.Cod_func ,
			'Avaliação e seleção' AS Modulo ,
			'VAGAS e-talent' AS Produto ,
			A.Tipo_lancamento + ' ' + A.Tipo_Rel_Comp_Rel AS Acao ,
			A.Data_lancamento ,
			SUM(A.Qtd_lancamentos) AS Qtd
	FROM	[VAGAS_DW].[VAGAS_DW].[VAGAS_ETALENT_LANCAMENTOS] AS A		INNER JOIN [hrh-data].[dbo].[Clientes] AS B ON A.Cod_cli = B.Cod_cli
																		INNER JOIN [hrh-data].[dbo].[Funcionarios] AS C ON B.Cod_cli = C.CodCli_func
	WHERE	A.Data_lancamento &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			B.IdContaCRM_cli ,
			A.Cod_cli ,
			C.Cod_func ,
			A.Tipo_lancamento + ' ' + A.Tipo_Rel_Comp_Rel ,
			A.Data_lancamento
	UNION ALL
	SELECT	B.IdContaCRM_cli ,
			A.Cod_cli ,
			A.Cod_func ,
			'Avaliação e seleção' AS Modulo ,
			'Ficha' AS Produto ,
			'Solicitou preenchidamento na candidatura' AS Acao ,
			CONVERT(DATE, A.Data_publicacao) AS [Data] ,
			COUNT(*) AS Qtd
	FROM	[VAGAS_DW].[VAGAS] AS A		INNER JOIN [hrh-data].[dbo].[Clientes] AS B ON A.Cod_cli = B.Cod_cli
	WHERE	A.SOLICITA_PREENCHIMENTO_FICHA = 'SIM'
			AND A.Data_publicacao &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			B.IdContaCRM_cli ,
			A.Cod_cli ,
			A.Cod_func ,
			A.Data_publicacao
	UNION ALL
	SELECT	B.IdContaCRM_cli ,
			A.Cod_cli ,
			A.Cod_func ,
			'Avaliação e seleção' AS Modulo ,
			'Teste' AS Produto ,
			'Solicitou preenchimento na candidatura' AS Acao ,
			CONVERT(DATE, A.Data_publicacao) AS [Data] ,
			COUNT(*) AS Qtd
	FROM	[VAGAS_DW].[VAGAS] AS A		INNER JOIN [hrh-data].[dbo].[Clientes] AS B ON A.Cod_cli = B.Cod_cli
	WHERE	A.SOLICITA_TESTE = 'SIM'
			AND A.Data_publicacao &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			B.IdContaCRM_cli ,
			A.Cod_cli ,
			A.Cod_func ,
			A.Data_publicacao
	UNION ALL
	SELECT	B.IdContaCRM_cli ,
			A.Cod_cli ,
			A.Cod_func ,
			'Atração de talentos' AS Modulo ,
			'Vaga' AS Produto ,
			'Publicou' + ' - ' + A.Tipo_processo AS Acao ,
			CONVERT(DATE, A.Data_publicacao) AS [Data] ,
			COUNT(*) AS Qtd
	FROM	[VAGAS_DW].[VAGAS] AS A		INNER JOIN [hrh-data].[dbo].[Clientes] AS B ON A.Cod_cli = B.Cod_cli
	WHERE	A.Data_publicacao &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			B.IdContaCRM_cli ,
			A.Cod_cli ,
			A.Cod_func ,
			A.Tipo_processo ,
			A.Data_publicacao
	UNION ALL
	SELECT	C.IdContaCRM_cli ,
			C.Cod_cli ,
			B.CodFunc_vaga AS Cod_func ,
			'Avaliação e seleção' AS Modulo ,
			'Currículo' AS Produto ,
			'Avaliou' + ' --&gt; ' + A.Avaliacao AS Acao ,
			A.Data_avaliacao AS [Data] ,
			SUM(A.Qtd_avaliacoes_cvs) AS Qtd
	FROM	[VAGAS_DW].[AVALIACOES_CVS] AS A		INNER JOIN [hrh-data].[dbo].[Vagas] AS B ON A.Cod_vaga = B.Cod_vaga
													INNER JOIN [hrh-data].[dbo].[Clientes] AS C ON B.CodCliente_vaga = C.Cod_cli
	WHERE	A.Data_avaliacao &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			C.IdContaCRM_cli ,
			C.Cod_cli ,
			B.CodFunc_vaga ,
			A.Avaliacao ,
			A.Data_avaliacao
	UNION ALL
	SELECT	B.IdContaCRM_cli AS IdConta_CRM ,
			A.Cod_cli ,
			A.Cod_func ,
			'Engajamento' AS Modulo ,
			'SMS' AS Produto ,
			'Envio' AS Acao ,
			CONVERT(DATE, A.Data_envio) AS [Data] ,
			SUM(A.QTD_SMS_ENVIADO) AS Qtd
	FROM	[VAGAS_DW].[VAGAS_DW].[ENVIO_SMS_VEP] AS A		INNER JOIN [hrh-data].[dbo].[Clientes] AS B ON A.Cod_cli = B.Cod_cli
	WHERE	A.Data_envio &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			B.IdContaCRM_cli ,
			A.Cod_cli ,
			A.Cod_func ,
			CONVERT(DATE, A.Data_envio) 
	UNION ALL
	SELECT	C.IdContaCRM_cli AS IdConta_CRM ,
			C.Cod_cli ,
			A.Cod_func ,
			'Avaliação e seleção' Modulo ,
			'Evento Presencial' AS Produto ,
			'Agendamento' AS Acao ,
			A.Data_envio_agendamento AS [Data] ,
			SUM(A.Qtd_envio_agendamento) AS Qtd
	FROM	[VAGAS_DW].[VAGAS_DW].[EVENTOS_PRESENCIAIS] AS A		INNER JOIN [hrh-data].[dbo].[Funcionarios] AS B ON A.Cod_func = B.Cod_func
																	INNER JOIN [hrh-data].[dbo].[Clientes] AS C ON B.CodCli_func = C.Cod_cli
	WHERE	A.Data_envio_agendamento &gt; '" + @[User::DATA_CARGA_INICIO] + "'
	GROUP BY
			C.IdContaCRM_cli ,
			C.Cod_cli ,
			A.Cod_func ,
			A.Data_envio_agendamento
) AS Z
WHERE Z.DATA &lt;= GETDATE()
AND Z.IDCONTA_CRM in (SELECT id_clientecrm FROM VAGAS_DW.TMP_CLIENTES_SENSEDATA)
GO"</DTS:PropertyExpression>
