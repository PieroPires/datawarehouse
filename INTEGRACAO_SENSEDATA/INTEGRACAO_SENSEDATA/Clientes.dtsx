      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{01D8D567-93B2-4E49-A51A-28782164D7C4}"
          SQLTask:SqlStatementSource="-- Campos Rescisão Opp:&#xA;DROP TABLE IF EXISTS TMP_Tab_Opp_Rescisao_Motivos;&#xA;&#xA;SELECT&#x9;A.CONTAID ,&#xA;&#x9;&#x9;A.OportunidadeID ,&#xA;&#x9;&#x9;ISNULL(B.Concorrente_Outro, '') AS Concorrente_outro ,&#xA;&#x9;&#x9;ISNULL(B.Outra_empresa_cliente, '') AS Outra_empresa_cliente&#xA;INTO&#x9;TMP_Tab_Opp_Rescisao_Motivos&#xA;FROM&#x9;[VAGAS_DW].[OPORTUNIDADES] AS A&#x9;&#x9;INNER JOIN OPENQUERY(MYSQLPROVIDER, 'SELECT&#x9;A1.id_c AS OportunidadeID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.concorrente_outro_c AS Concorrente_Outro ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.outra_empresa_cliente_c AS Outra_empresa_cliente&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9; FROM&#x9;sugarcrm.opportunities_cstm AS A1') AS B&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON A.OportunidadeID = B.OportunidadeID COLLATE SQL_Latin1_General_CP1_CI_AI&#xA;WHERE&#x9;EXISTS ( SELECT&#x9;1&#xA;&#x9;&#x9;&#x9;&#x9; FROM&#x9;[VAGAS_DW].[CLIENTES] AS A1&#xA;&#x9;&#x9;&#x9;&#x9; WHERE&#x9;A.CONTAID = A1.CONTA_ID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.EX_CLIENTE = 0)&#xA;&#x9;&#x9;AND A.OportunidadeCategoria = 'rescisão'&#xA;&#x9;&#x9;AND A.Fase = 'fechado_e_ganho' &#xA;&#x9;&#x9;AND A.RECORRENTE = 1 ;&#xA;&#xA;CREATE NONCLUSTERED INDEX IDX_TMP_Tab_Opp_Rescisao_Motivos_OportunidadeID ON TMP_Tab_Opp_Rescisao_Motivos (OportunidadeID) ;&#xA;&#xA;TRUNCATE TABLE [VAGAS_DW].[TMP_CLIENTES_SENSEDATA];&#xA;&#xA;INSERT INTO [VAGAS_DW].[TMP_CLIENTES_SENSEDATA]&#xA;SELECT&#x9;&#xA;&#x9;&#x9;(SELECT CLIENTE_CRM FROM VAGAS_DW.CLIENTES WHERE CONTA_ID = G.CONTA_ID_PAI) AS GrupoEconomico ,&#xA;&#x9;&#x9;A.CONTA_ID AS Id_ClienteCRM ,&#xA;&#x9;&#x9;A.CLIENTE_CRM AS ClienteCRM ,&#xA;&#x9;&#x9;B.DescricaoCliente ,&#xA;&#x9;&#x9;C.CNPJ ,&#xA;&#x9;&#x9;CONVERT(DATE, D.DataFechamento) AS Cliente_desde ,&#xA;&#x9;&#x9;G.MUNICIPIO as Municipio,&#xA;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Acre' OR G.UF = 'AC' THEN 'AC'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Alagoas' OR G.UF = 'AL' THEN 'AL'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Amapá' OR G.UF = 'AP' THEN 'AP'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Amazonas' OR G.UF = 'AM' THEN 'AM'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Bahia' OR G.UF = 'BA' THEN 'BA'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Ceará' OR G.UF = 'CE' THEN 'CE'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Distrito Federal' OR G.UF = 'DF' THEN 'DF'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Espírito Santo' OR G.UF = 'ES' THEN 'ES'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Goiás' OR G.UF = 'GO' THEN 'GO'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Maranhão' OR G.UF = 'MA' THEN 'MA'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Mato Grosso' OR G.UF = 'MT' THEN 'MT'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Mato Grosso do Sul' OR G.UF = 'MS' THEN 'MS'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Minas Gerais' OR G.UF = 'MG' THEN 'MG'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Pará' OR G.UF = 'PA' THEN 'PA'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Paraíba' OR G.UF = 'PB' THEN 'PB'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Paraná' OR G.UF = 'PR' THEN 'PR'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Pernambuco' OR G.UF = 'PE' THEN 'PE'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Piauí' OR G.UF = 'PI' THEN 'PI'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Rio de Janeiro' OR G.UF = 'RJ' THEN 'RJ'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Rio Grande do Norte' OR G.UF = 'RN' THEN 'RN'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Rio Grande do Sul' OR G.UF = 'RS' THEN 'RS'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Rondônia' OR G.UF = 'RO' THEN 'RO'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Roraima' OR G.UF = 'RR' THEN 'RR'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Santa Catarina' OR G.UF = 'SC' THEN 'SC'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'São Paulo' OR G.UF = 'SP' THEN 'SP'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Sergipe' OR G.UF = 'SE' THEN 'SE'&#xA;&#x9;&#x9;&#x9;WHEN G.UF = 'Tocantins' OR G.UF = 'TO' THEN 'TO'&#xA;&#x9;&#x9;&#x9;ELSE NULL&#xA;&#x9;&#x9;END AS Estado,&#xA;&#x9;&#x9;A.SEGMENTO_SC AS Porte ,&#xA;&#x9;&#x9;NULL AS Fase ,&#xA;&#x9;&#x9;A.CNAE_SECAO AS Segmento ,&#xA;&#x9;&#x9;A.Classificacao AS [Status] ,&#xA;&#x9;&#x9;NULL AS CSM ,&#xA;&#x9;&#x9;IIF(CHARINDEX('.', GR) = 0, GR, UPPER(LEFT(GR, 1)) + LOWER(SUBSTRING(GR, 2, (LEN(SUBSTRING(GR, 1, CHARINDEX('.', GR)-1))-1))) + ' ' + UPPER(SUBSTRING(GR, (CHARINDEX('.', GR)+1), 1)) + LOWER(REVERSE(LEFT(SUBSTRING(REVERSE(GR), 1, CHARINDEX('.', REVERSE(GR))-1), LEN(SUBSTRING(REVERSE(GR), 1, CHARINDEX('.', REVERSE(GR))-1))-1)))) AS GR ,&#xA;&#x9;&#x9;IIF(CHARINDEX('.', D.Proprietario) = 0, D.Proprietario, UPPER(LEFT(D.Proprietario, 1)) + LOWER(SUBSTRING(D.Proprietario, 2, (LEN(SUBSTRING(D.Proprietario, 1, CHARINDEX('.', D.Proprietario)-1))-1))) + ' ' + UPPER(SUBSTRING(D.Proprietario, (CHARINDEX('.', D.Proprietario)+1), 1)) + LOWER(REVERSE(LEFT(SUBSTRING(REVERSE(D.Proprietario), 1, CHARINDEX('.', REVERSE(D.Proprietario))-1), LEN(SUBSTRING(REVERSE(D.Proprietario), 1, CHARINDEX('.', REVERSE(D.Proprietario))-1))-1)))) AS Proprietario_Opp ,&#xA;&#x9;&#x9;E.DataFimUso AS Data_FimUso ,&#xA;&#x9;&#x9;CONCAT_WS(' / ', F.MOTIVO_FECHAMENTO, F.MOTIVO_FECHAMENTO_DETALHE) AS Motivo_cancelamento,&#xA;&#x9;&#x9;CASE&#xA;&#x9;&#x9;&#x9;WHEN F.MOTIVO_FECHAMENTO = 'utiliza_outro_sistema'&#xA;&#x9;&#x9;&#x9;&#x9;THEN ISNULL(F.MOTIVO_CONCORRENTE, F.Concorrente_Outro)&#xA;&#x9;&#x9;&#x9;WHEN F.MOTIVO_FECHAMENTO = 'fusao_entre_empresas'&#xA;&#x9;&#x9;&#x9;&#x9;THEN F.Outra_empresa_cliente&#xA;&#x9;&#x9;&#x9;ELSE ''&#xA;&#x9;&#x9;END AS Concorrente_fusao ,&#xA;&#x9;&#x9;G.TIPO AS TIPO_CONTA ,&#xA;&#x9;&#x9;G.CATEGORIA AS CATEGORIA_CONTA&#xA;FROM&#x9;[VAGAS_DW].[CLIENTES] AS A&#x9;&#x9;OUTER APPLY ( SELECT&#x9;TOP 1 A1.RAZAO_SOCIAL AS DescricaoCliente&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  FROM&#x9;&#x9;[VAGAS_DW].[Clientes_DadosERP] AS A1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  WHERE&#x9;&#x9;A.CONTA_ID = A1.IdCliente_CRM OR A.CONTA_ID = A1.IdCliente_CRM_Filho ) AS B&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;OUTER APPLY ( SELECT&#x9;TOP 1 A1.CNPJ&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  FROM&#x9;&#x9;[VAGAS_DW].[FATURAS] AS A1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  WHERE&#x9;&#x9;A.CONTA_ID = A1.COD_CLI_CRM&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  ORDER BY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.DATA_REFERENCIA DESC ) AS C&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;OUTER APPLY ( SELECT&#x9;TOP 1 *&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  FROM&#x9;&#x9;[VAGAS_DW].[OPORTUNIDADES] AS A1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  WHERE&#x9;&#x9;A.CONTA_ID = A1.CONTAID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.Fase = 'fechado_e_ganho'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.OportunidadeCategoria IN ('cliente_potencial', 'cliente_cotacao')&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  ORDER BY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.DataFechamento ) AS D&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT OUTER JOIN OPENQUERY(MYSQLPROVIDER, 'SELECT&#x9;A2.id_c AS CONTA_ID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.data_fim_uso_c AS DataFimUso&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  FROM&#x9;&#x9;oqc_offering_cstm AS A1&#x9;INNER JOIN accounts_cstm AS A2&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON A1.id_c = A2.oqc_offering_id_c') AS E&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON A.CONTA_ID = E.CONTA_ID COLLATE SQL_Latin1_General_CP1_CI_AI&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;OUTER APPLY ( SELECT&#x9;TOP 1 A1.OportunidadeID ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.Oportunidade ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.DataFechamento AS DataUltRescisao ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.MOTIVO_FECHAMENTO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.MOTIVO_FECHAMENTO_DETALHE ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A1.MOTIVO_CONCORRENTE ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A2.CONCORRENTE_OUTRO COLLATE SQL_Latin1_General_CP1_CI_AI AS CONCORRENTE_OUTRO ,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A2.Outra_empresa_cliente COLLATE SQL_Latin1_General_CP1_CI_AI AS Outra_empresa_cliente&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  FROM&#x9;&#x9;[VAGAS_DW].[OPORTUNIDADES] AS A1&#x9;INNER JOIN TMP_Tab_Opp_Rescisao_Motivos AS A2 &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ON A1.OportunidadeID = A2.OportunidadeID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  WHERE&#x9;&#x9;A.CONTA_ID = A1.CONTAID&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.Fase = 'fechado_e_ganho'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.RECORRENTE = 1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;AND A1.OportunidadeCategoria = 'rescisão'&#xA;&#x9;&#x9;