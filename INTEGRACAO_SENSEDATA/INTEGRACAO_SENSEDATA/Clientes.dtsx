                  description="O comando SQL a ser executado."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select top 100
	null as grupo_economico
	,a.conta_id as id_cliente
	,cliente as [Descrição Cliente]
	,a.cnpj
	,a.CIDADE as municpio
	,a.UF as estado
	,a.SEGMENTO_SC as porte
	,null as fase
	,a.cnae_secao as segmento
	,null as status
	,null as csm
	,a.gr as cs
	,c.proprietario as comercial
	,case d.OportunidadeCategoria when 'rescisao' then d.DataFechamento else null end as data_canc
	,null as motivo_cancelamento
	,null as observacao_cancelamento
	,null as concorrente_fusao
from vagas_dw.clientes as a
	outer apply (select top 1 cliente from vagas_dw.faturamento where cod_cli_crm = a.conta_id order by data_emissao desc) as b
	outer apply (select top 1 proprietario from vagas_dw.OPORTUNIDADES where contaid = a.conta_id and fase = 'fechado_e_ganho' order by DataFechamento desc) as c
	outer apply (select top 1 OportunidadeCategoria, datafechamento from vagas_dw.OPORTUNIDADES  where contaid = a.conta_id order by DataFechamento desc) as d</property>
