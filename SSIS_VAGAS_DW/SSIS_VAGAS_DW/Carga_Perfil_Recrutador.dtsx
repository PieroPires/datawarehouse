      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="[McDuck].[SqlCommand]">"SELECT	A.id AS Id ,
		CASE
        	WHEN A.status = 1
            	THEN 'Ativo'
            WHEN A.status = 0
            	THEN 'Inativo'
            ELSE NULL
		END AS Status ,
        CASE
        	WHEN A.plan_type = 1
            	THEN 'Anual'
            WHEN A.plan_type = 0
            	THEN 'Mensal'
            ELSE NULL
        END AS Plano ,
        CAST(A.next_insertion AS DATE) AS DtProx_Credito ,
        A.credit_quantity AS Qtd_credito_contratado ,
		TO_CHAR(A.created_at::date, 'DD/MM/YYYY') || ' até ' || TO_CHAR(CAST(A.created_at::date + CAST('1 year' AS INTERVAL) AS DATE), 'DD/MM/YYYY') AS Vigencia_contrato ,
		CASE
        	WHEN A.automatic_renewal = true
            	THEN 'Automática'
            WHEN A.automatic_renewal = false
            	THEN 'Manual'
            ELSE NULL
        END AS Renovacao ,
        A.updated_at::date AS DtUltAtualizacao_perfil
FROM	public.recurrent_plans AS A
WHERE	CAST(REPLACE(CAST(A.updated_at AS CHAR(10)), '-', '') AS INTEGER) &gt;" +  @[User::LastUpdated]</DTS:PropertyExpression>
