      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="[Chat].[SqlCommand]">"
SELECT	messages.id 				AS id_mensagem,	
        messages.direct_message_id 	AS id_chat,
        messages.inserted_at 		AS data_envio_mensagem,
        messages.read_at 			AS data_leitura_mensagem,
       	sender.vagas_id				AS id_origem_mensagem,
        CASE
        	WHEN sender.role = 'applicant'
            	THEN 'Candidato'
            WHEN sender.role = 'applicant_in_company'
                THEN 'Candidato em navegação exclusiva'
            WHEN sender.role = 'employee'
         		THEN 'Empresa'
         END AS origem_mensagem,
        target.vagas_id				AS id_destino_mensagem,
        CASE
        	WHEN target.role = 'applicant'
            	THEN 'Candidato'
            WHEN target.role = 'applicant_in_company'
                THEN 'Candidato em navegação exclusiva'
            WHEN target.role = 'employee'
         		THEN 'Empresa'
         END AS destino_mensagem,
        direct_messages.company_id	AS codcli_chat,
        NULL AS resposta_candidato_chat,
		 CASE
        	WHEN sender.role = 'employee'
            	THEN sender.vagas_id
            WHEN target.role = 'employee'
            	THEN target.vagas_id
            ELSE NULL
          END AS codfunc_chat,
         CASE
         	WHEN sender.role IN ('applicant', 'applicant_in_company')
            	THEN sender.vagas_id
            WHEN target.role IN ('applicant', 'applicant_in_company')
            	THEN target.vagas_id
            ELSE NULL
          END AS codcand_chat,
		  messages.type AS Tipo_mensagem
FROM	messages 	INNER JOIN direct_messages
					ON messages.direct_message_id = direct_messages.id	
                    INNER JOIN users as sender
                    ON messages.origin_id = sender.id
                    INNER JOIN users as target
                    ON messages.target_id = target.id
WHERE	messages.id &gt;" +  @[User::Id_UltMsg]  +
	"OR messages.read_at &gt;=" + "'"  + (DT_WSTR, 50)@[User::Data_UltLeituraMsg]  + "'"</DTS:PropertyExpression>
