       CONVERT(D.user_name USING Latin1) AS USUARIO_CADASTRO,
       A.deleted AS FLAG_DELETADO,
       IF(CONVERT_TZ(A.date_sent,@@session.time_zone, '-03:00') &lt; '1900-01-01', '1900-01-01', CONVERT_TZ(A.date_sent,@@session.time_zone, '-03:00')) AS DATA_ENVIO,
       CONVERT(A.name USING Latin1) AS NOME,
       CONVERT(A.type USING Latin1) AS TIPO,
       CONVERT(A.status USING Latin1) AS STATUS,
       CONVERT(CASE WHEN E.id IS NOT NULL THEN E.name
            WHEN F.id IS NOT NULL THEN F.name
            WHEN G.id IS NOT NULL THEN G.name
            WHEN H.id IS NOT NULL THEN H.name
            WHEN I.id IS NOT NULL THEN CONCAT(IFNULL(I.first_name,\"\"),\" \",IFNULL(I.last_name,\"\"))
            WHEN J.id IS NOT NULL THEN CONCAT(IFNULL(J.first_name,\"\"),\" \",IFNULL(J.last_name,\"\"))
            ELSE \"\" END USING Latin1) AS REFERENTE_A, 
       CONVERT(A.parent_type USING Latin1) AS TIPO_CONTATO,
       L.name AS ANEXO ,
       CONVERT(A.parent_id USING Latin1) AS ID_TIPO_CONTATO ,
	   CONVERT(CONCAT(LTRIM(RTRIM(O.first_name)), \" \", LTRIM(RTRIM(O.last_name))) USING Latin1) AS NOME_CONTATO ,
       CONVERT(LTRIM(RTRIM(N1.email_address)) USING Latin1) AS EMAIL_CONTATO ,
       CONVERT(IF(P.contato_principal_c = 1 OR P.usuario_admin_c = 1, \"SIM\", \"NÃO\") USING Latin1) AS CONTATO_ADM_PRINCIPAL ,

	   ( SELECT CASE
                WHEN A1.bean_module = \"Accounts\"
                    THEN CONVERT(A1.bean_module USING Latin1)
                WHEN A1.bean_module = \"Opportunities\"
                    THEN CONVERT(A1.bean_module USING Latin1)
                WHEN A1.bean_module = \"Cases\"
                    THEN CONVERT(A1.bean_module USING Latin1)
                WHEN A1.bean_module = \"Leads\"
                    THEN CONVERT(A1.bean_module USING Latin1)
                WHEN A1.bean_module = \"Contacts\"
                    THEN CONVERT(A1.bean_module USING Latin1)
            END AS CONTEXTO                    
      FROM   sugarcrm.emails_beans AS A1
      WHERE  A.id = A1.email_id
      ORDER BY
              A1.bean_module
      LIMIT 1) AS CONTEXTO ,
      
      ( SELECT CASE
                WHEN A1.bean_module = \"Accounts\"
                    THEN CONVERT(A1.bean_id USING Latin1)
                WHEN A1.bean_module = \"Opportunities\"
                    THEN CONVERT(A1.bean_id USING Latin1)
                WHEN A1.bean_module = \"Cases\"
                    THEN CONVERT(A1.bean_id USING Latin1)
                WHEN A1.bean_module = \"Leads\"
                    THEN CONVERT(A1.bean_id USING Latin1)
                WHEN A1.bean_module = \"Contacts\"
                    THEN CONVERT(A1.bean_id USING Latin1)
            END AS ID_CONTEXTO                    
      FROM   sugarcrm.emails_beans AS A1
      WHERE  A.id = A1.email_id
      ORDER BY
              A1.bean_module
      LIMIT 1) AS ID_CONTEXTO 



FROM emails A
INNER JOIN users D ON D.id = A.created_by 
LEFT OUTER JOIN users B ON B.id = A.assigned_user_id
LEFT OUTER JOIN users C ON C.id = A.modified_user_id
LEFT OUTER JOIN cases E ON E.id = A.parent_id 
                 AND A.parent_type = \"Cases\"
LEFT OUTER JOIN oqc_contract F ON F.id = A.parent_id 
                 AND A.parent_type = \"oqc_Contract\"
LEFT OUTER JOIN opportunities G ON G.id = A.parent_id 
                 AND A.parent_type = \"Opportunities\"
LEFT OUTER JOIN accounts H ON H.id = A.parent_id 
                 AND A.parent_type = \"Accounts\"                               
LEFT OUTER JOIN contacts I ON I.id = A.parent_id 
                 AND A.parent_type = \"Contacts\"                                                              
LEFT OUTER JOIN leads J ON J.id = A.parent_id 
                 AND A.parent_type = \"Leads\" 
LEFT OUTER JOIN notes L ON L.parent_id = A.id
                       AND L.parent_type = \"Emails\"
                       AND L.deleted = 0
                       AND L.file_mime_type = \"application/pdf\"
                       AND L.id = ( SELECT MIN(L1.id) 
                  FROM notes L1
                                    WHERE L1.parent_id = A.id
                                    AND L1.parent_type = \"Emails\"
                                    AND L1.deleted = 0
                                    AND L1.file_mime_type = \"application/pdf\" )  
									
LEFT OUTER JOIN	sugarcrm.emails_email_addr_rel AS M	ON M.email_id = A.id AND M.address_type = \"to\" AND M.deleted = 0
LEFT OUTER JOIN sugarcrm.email_addr_bean_rel AS N ON N.email_address_id = M.email_address_id AND N.bean_module = \"Contacts\" AND N.deleted = 0
LEFT OUTER JOIN sugarcrm.email_addresses AS N1 ON N1.id = N.email_address_id  AND N1.deleted = 0
LEFT OUTER JOIN sugarcrm.contacts AS O ON O.id = N.bean_id AND O.deleted = 0
LEFT OUTER JOIN sugarcrm.contacts_cstm AS P ON O.id = P.id_c AND P.funcionario_desligado_c = 0						
									               
WHERE A.date_modified &gt; " + @[User::DATA_ALTERACAO] + 

