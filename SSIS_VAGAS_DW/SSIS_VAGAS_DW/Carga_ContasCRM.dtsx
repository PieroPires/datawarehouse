                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand">SELECT	CONVERT(A.id USING Latin1) AS IdConta_CRM  ,
		CONVERT(A.name USING Latin1) AS NomeConta_CRM ,
		CONVERT(replace(replace(replace(B.cnpj_c, '/',''),'.',''),'-','') USING Latin1) AS NumCPNJConta_CRM ,
		CONVERT(B.tipo_c USING Latin1) AS TipoConta_CRM ,
        CONVERT(A.account_type USING Latin1) AS CategoriaConta_CRM ,
		A.date_entered AS DataCriacaoConta_CRM ,
        B.porcentual_rateio_c AS PercentualRateioConta_CRM ,
        B.valor_principal_c AS ValorPrincipalConta_CRM ,
        CONVERT(B.mercado_c USING Latin1) AS MercadoConta_CRM ,
        CONVERT(D.user_name USING Latin1) AS ProprietarioConta_CRM ,
        CONVERT(B.id_vagas_c USING Latin1) AS IDVAGAS_Conta_CRM ,
        CONVERT(B.cnae_secao_id_c USING Latin1) AS CNAE_SECAO_ID_CRM ,
        CONVERT(B.cnae_secao_descr_c USING Latin1) AS CNAE_SECAO_CRM ,
        CONVERT(B.cnae_divisao_id_c USING Latin1) AS CNAE_DIVISAO_ID_CRM ,
        CONVERT(B.cnae_divisao_descr_c USING Latin1) AS CNAE_DIVISAO_CRM ,
        CONVERT(B.cnae_classe_id_c USING Latin1) AS CNAE_CLASSE_ID_CRM ,
        CONVERT(B.cnae_classe_descr_c USING Latin1) AS CNAE_CLASSE_CRM ,
        CONVERT(B.cnae_faixa_funcionarios_c USING Latin1) AS CNAE_FAIXA_FUNCIONARIOS_CRM ,
        CONVERT(B.cnae_subclasse_id_c USING Latin1) AS CNAE_SUBCLASSE_ID_C_CRM ,
        CONVERT(B.cnae_subclasse_descr_c USING Latin1) AS CNAE_SUBCLASSE_DESCR_C_CRM ,
        CONVERT(B.segmento_c USING Latin1) AS SegmentoSCConta_CRM ,
        CONVERT(B.consultoria_rh_c USING Latin1) AS ConsultoriaConta_CRM ,
        CONVERT(A.parent_id USING Latin1) AS IdContaPai_CRM
FROM	sugarcrm.accounts AS A	LEFT OUTER JOIN sugarcrm.accounts_cstm AS B ON A.id = B.id_c
								LEFT OUTER JOIN sugarcrm.users AS C ON A.created_by = C.id
                                LEFT OUTER JOIN sugarcrm.users AS D ON A.assigned_user_id = D.id
WHERE	A.deleted = 0 ;</property>
