    <DataSource Name="SQLMirror">
      <DataSourceReference>SQLMirror</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>117c5f05-dfb8-4936-9986-5dbd4897b301</rd:DataSourceID>
