  <DataSets>
    <DataSet Name="Mailing_clientes_ativos">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <CommandText>SELECT        SUB.DATA_EXTRACAO, SUB.COD_FUNC, SUB.NOME_FUNC, SUB.EMAIL_FUNC, SUB.NOME_CLIENTE, SUB.MERCADO
FROM            (SELECT        CONVERT(SMALLDATETIME, CONVERT(VARCHAR, GETDATE(), 112)) AS DATA_EXTRACAO, A.Cod_func AS COD_FUNC, REPLACE(REPLACE(REPLACE(LTRIM(RTRIM(A.Nome_func)), CHAR(9), ''), 
                                                    CHAR(10), ''), CHAR(13), '') AS NOME_FUNC, REPLACE(REPLACE(REPLACE(LTRIM(RTRIM(A.Email_func)), CHAR(9), ''), CHAR(10), ''), CHAR(13), '') AS EMAIL_FUNC, ISNULL(B.CLIENTE_CRM, 
                                                    B.CLIENTE_VAGAS) AS NOME_CLIENTE, B.MERCADO, ROW_NUMBER() OVER (PARTITION BY A.Email_func
                          ORDER BY A.DtAtualizacao_func DESC) AS CONTROLE
FROM            [hrh-data].[dbo].[Funcionarios] AS A INNER JOIN
                         [VAGAS_DW].[VAGAS_DW].[CLIENTES] AS B ON A.CodCli_func = B.COD_CLI INNER JOIN
                         [hrh-data].[dbo].[Clientes] AS C ON A.CodCli_func = C.Cod_cli
WHERE        B.EX_CLIENTE = 0 AND EXISTS
                             (SELECT        1
                               FROM            [hrh-data].[dbo].[Clientes-Login] AS A1
                               WHERE        A.Cod_func = A1.Codfunc_log AND A1.Op_log = 1 AND A1.Data_log &gt;= DATEADD(YEAR, - 1, CAST(GETDATE() AS DATE)) AND A1.CodUsuManut_log = 0) AND B.CLIENTE_VAGAS != 'gruposVagas' AND 
                         A.Email_func NOT LIKE '%@vagas.com%' AND A.Removido_func = 0 AND ISNULL(A.Email_func, '') != '' AND C.OptInVagasExpress_cli = 1 AND A.OptInVagasExpress_func = 1 AND 
                         C.OptInVagasAcontece_cli = 1 AND A.OptInVagasAcontece_func = 1) AS SUB
WHERE        SUB.CONTROLE = 1 AND SUB.EMAIL_FUNC NOT LIKE '%@teste%'
ORDER BY SUB.NOME_FUNC ASC, SUB.EMAIL_FUNC ASC;</CommandText>
      </Query>
