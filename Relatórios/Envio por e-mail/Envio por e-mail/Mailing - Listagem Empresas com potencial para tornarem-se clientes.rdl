  <DataSets>
    <DataSet Name="DATASET_1">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PRM_DIA">
            <Value>=Parameters!PRM_DIA.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ULT_DIA">
            <Value>=Parameters!ULT_DIA.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	A.CONTA ,
		B.NOME ,
		B.SOBRENOME ,
		B.EMAIL ,
		A.DATA_ENTRADA
FROM	(
			SELECT	A.CONTA_ID ,
					A.CONTA_CRM AS CONTA ,
					A.TIPO ,
					A.CATEGORIA ,
					A.CRIADO_POR ,
					CONVERT(DATE, A.DATA_ENTRADA) AS DATA_ENTRADA ,
					A.POSSUI_OPORTUNIDADE
			FROM	[VAGAS_DW].[VAGAS_DW].[CONTAS_CRM] AS A
			UNION ALL
			SELECT	A.CONTA_ID ,
					A.CONTA_CRM AS CONTA ,
					A.TIPO ,
					A.CATEGORIA ,
					A.CRIADO_POR ,
					CONVERT(DATE, A.DATA_ENTRADA) AS DATA_ENTRADA ,
					A.POSSUI_OPORTUNIDADE 
			FROM	[VAGAS_DW].[VAGAS_DW].[CONTAS_MEMBRO_CRM] AS A ) AS A		CROSS APPLY (SELECT	TOP 1 A1.NOME ,
																										  A1.SOBRENOME ,
																										  A1.EMAIL
																							 FROM	[VAGAS_DW].[VAGAS_DW].[CONTATOS_CRM] AS A1
																							 WHERE	A.CONTA_ID = A1.COD_CONTA_CRM
																							 ORDER BY
																									A1.DATA_MODIFICACAO DESC) AS B
WHERE	EXISTS (SELECT	1
				FROM	[VAGAS_DW].[VAGAS_DW].[GRUPO_VENDEDORES] AS A1
				WHERE	A.CRIADO_POR = A1.VENDEDOR
						AND A1.GRUPO_VENDEDOR = 'LEADs')
		AND A.POSSUI_OPORTUNIDADE = 'NÃO'
		AND A.DATA_ENTRADA &gt;= @PRM_DIA
		AND A.DATA_ENTRADA &lt; @ULT_DIA
		AND NOT EXISTS (SELECT	1
						FROM	[VAGAS_DW].[VAGAS_DW].[CLIENTES] AS A1
						WHERE	A.CONTA_ID = A1.CONTA_ID
								AND A1.EX_CLIENTE = 0)
		AND (A.TIPO = 'cliente_potencial'
			 OR A.CATEGORIA = 'cliente_potencial')
ORDER BY
		A.DATA_ENTRADA ASC ;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CONTA">
          <DataField>CONTA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NOME">
          <DataField>NOME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SOBRENOME">
          <DataField>SOBRENOME</DataField>
