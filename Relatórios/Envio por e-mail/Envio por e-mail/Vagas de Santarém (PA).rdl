  <DataSources>
    <DataSource Name="SERVER_MIRROR">
      <DataSourceReference>SQLMirror</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4c812f44-1b25-474c-ad75-1bc8e6481331</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DATASET_VAGAS_SANTAREM_PA">
      <Query>
        <DataSourceName>SERVER_MIRROR</DataSourceName>
        <CommandText>SELECT	B.Nome_cli AS [Nome da empresa]
		, D.Descr_formMax AS [Escolaridade da vaga]
		, E.Descr_hierarquia AS [Nível da vaga]
		, A.Cod_vaga AS [Código da vaga]
		, A.Cargo_vaga AS [Cargo da vaga]
		, CONCAT('http://vagas.com.br/vagas/v', A.Cod_vaga) AS URL
		, A.DescrReg_vaga AS [Cidade/Estado/País] ,
		LTRIM(
		ISNULL(F1.Descr_segmento, ' ') + IIF(F1.Descr_segmento IS NULL, ' ', ', ') + ISNULL(F2.Descr_segmento, ' ') + IIF(F2.Descr_segmento IS NULL, ' ', ', ') +
		ISNULL(F3.Descr_segmento, ' ') + IIF(F3.Descr_segmento IS NULL, ' ', ', ') + ISNULL(F4.Descr_segmento, ' ') + IIF(F4.Descr_segmento IS NULL, ' ', ', ') +
		ISNULL(F5.Descr_segmento, ' ') + IIF(F5.Descr_segmento IS NULL, ' ', ', ') + ISNULL(F6.Descr_segmento, ' ') + IIF(F6.Descr_segmento IS NULL, ' ', ', ') +
		ISNULL(F7.Descr_segmento, ' ') + IIF(F7.Descr_segmento IS NULL, ' ', ', ') + ISNULL(F8.Descr_segmento, ' ') + IIF(F8.Descr_segmento IS NULL, ' ', ', ') +
		ISNULL(F9.Descr_segmento, ' ') + IIF(F9.Descr_segmento IS NULL, ' ', ', ') + ISNULL(F10.Descr_segmento, ' ') + IIF(F10.Descr_segmento IS NULL, ' ', ', ') +
		ISNULL(F11.Descr_segmento, ' ') + IIF(F11.Descr_segmento IS NULL, ' ', ', ') + ISNULL(F12.Descr_segmento, ' ') + IIF(F12.Descr_segmento IS NULL, ' ', ', ') +
		ISNULL(F13.Descr_segmento, ' ') + IIF(F13.Descr_segmento IS NULL, ' ', ', ') + ISNULL(F14.Descr_segmento, ' ') + IIF(F14.Descr_segmento IS NULL, ' ', ', ') +
		ISNULL(F15.Descr_segmento, ' ') + IIF(F15.Descr_segmento IS NULL, ' ', ', ') + ISNULL(F16.Descr_segmento, ' ') + IIF(F16.Descr_segmento IS NULL, ' ', ', '))
		AS SEGMENTOS_VAGA
FROM	[hrh-data].[dbo].[Vagas] AS A		INNER JOIN [hrh-data].[dbo].[Clientes] AS B	ON B.Cod_cli = A.CodCliente_vaga
											INNER JOIN [hrh-data].[dbo].[Divisoes] AS C ON C.Cod_div = A.CodDivVeic_vaga
											LEFT OUTER JOIN [hrh-data].[dbo].[Cad_formacaoMax] AS D ON D.Cod_formMax = A.CodFormacaoMin_vaga
											LEFT OUTER JOIN [hrh-data].[dbo].[Cad_hierarquias] AS E ON E.Cod_hierarquia = A.CodHierarquiaMin_vaga
											LEFT OUTER JOIN [hrh-data].[dbo].[Cad_segmentos] AS F1 ON A.CodSetor_vaga   = F1.Cod_segmento
											LEFT OUTER JOIN [hrh-data].[dbo].[Cad_segmentos] AS F2 ON A.CodSetor2_vaga = F2.Cod_segmento
											LEFT OUTER JOIN [hrh-data].[dbo].[Cad_segmentos] AS F3 ON A.CodSetor3_vaga = F3.Cod_segmento
