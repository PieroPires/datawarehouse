  <DataSets>
    <DataSet Name="DATASET_Novos_CVs">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <CommandText>select   nome_cand, email_cand
from     [hrh-data].dbo.Candidatos  
where    isnull(Ficticio_cand, 0) = 0  --&gt; flag para identificar os candidatos ficticios utilizados para treinamento 
         and isnull(FezAcessoIrrestrito_cand, 1) = 1 --&gt; 0 Exclusivo de Cliente BCE  - 1 Do vagas BCC (NESSE ESTUDO, NÃO IMPORTA SE É BCC OU BCE)  
         and EstadoReg_cand  = 1
         and dtCriacao_cand between dateadd(d, -7, getdate() - {fn current_time} + '07:00') and (getdate() - {fn current_time} + '07:00')
         and isnull(email_cand, '') &lt;&gt; ''
         and MalaDireta_cand = 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="nome_cand">
          <DataField>nome_cand</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="email_cand">
          <DataField>email_cand</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
