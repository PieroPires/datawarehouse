  <DataSets>
    <DataSet Name="DATASET_VAGAS_ASSESSORIA">
      <Query>
        <DataSourceName>SERVER_MIRROR</DataSourceName>
        <CommandText>------------------------------------------------------
-- Listagem com vagas de Pernambuco para a Assessoria:
------------------------------------------------------
SELECT	B.Nome_cli AS [Nome da empresa]
		, D.Descr_formMax AS [Escolaridade da vaga]
		, E.Descr_hierarquia AS [Nível da vaga]
		, A.Cod_vaga AS [Código da vaga]
		, A.Cargo_vaga AS [Cargo da vaga]
		, CONCAT('http://vagas.com.br/vagas/v', A.Cod_vaga) AS URL
		, A.DescrReg_vaga AS [Cidade/Estado/País]
FROM	[hrh-data].[dbo].[Vagas] AS A		INNER JOIN [hrh-data].[dbo].[Clientes] AS B		ON B.Cod_cli = A.CodCliente_vaga
											INNER JOIN [hrh-data].[dbo].[Divisoes] AS C ON C.Cod_div = A.CodDivVeic_vaga
											LEFT OUTER JOIN [hrh-data].[dbo].[Cad_formacaoMax] AS D ON D.Cod_formMax = A.CodFormacaoMin_vaga
											LEFT OUTER JOIN [hrh-data].[dbo].[Cad_hierarquias] AS E ON E.Cod_hierarquia = A.CodHierarquiaMin_vaga 
WHERE	A.Teste_vaga = 0 -- Não é teste
		AND A.Validada_vaga = 1 -- Vaga foi validada
		AND A.VagaModelo_vaga = 0 -- Vaga não é modelo
		AND A.VeiculacaoSuspensa_vaga = 0 --Veiculação não está suspensa
		AND A.DivulgacaoLiberada_vaga = 1 --Vagas com divulgação liberada
		AND A.ColetaCur_vaga = 0 -- Não é vaga de captação
		AND LEFT(A.Cargo_vaga, 5) != 'demo '
		AND LEFT(A.Cargo_vaga, 5) != 'demo-'
		AND Tipoprocesso_vaga != 1 -- Não é PET
		AND CodNavEx_div != 300 -- Não é RI
		AND CodNavEx_div != 400 -- Não é comunidade externa
		AND VagaComExt_vaga = 0 -- Não é comunidade externa
		AND MostraLinkEmpWeb_vaga = 1 -- Vagas não confidenciais
		AND (DtInicioWeb_vaga &lt; CAST(GETDATE() AS DATE) AND DATEDIFF(DAY, DtInicioWeb_vaga, CAST(GETDATE() AS DATE)) &lt;= 10) -- Vagas abertas nos últimos 10 dias
		AND (DtExpiracaoWeb_vaga &gt;= DATEADD(DAY, 15, CAST(GETDATE() AS DATE))) -- Vagas que expiram após 15 dias da data de extração
		AND A.CodUF_vaga = 16 -- Pernambuco
ORDER BY [Nome da empresa] ASC ;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
