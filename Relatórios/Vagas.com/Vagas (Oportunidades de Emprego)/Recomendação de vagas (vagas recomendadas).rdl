  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@COD_VAGA">
            <Value>=Parameters!COD_VAGA.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT	A.CodVaga_czMonitVagaCand AS COD_VAGA ,
		CONVERT(SMALLDATETIME ,CONVERT(VARCHAR, DataIni_czMonit, 112)) AS DATA_ENVIO ,
		COUNT(*) AS QTD_ALERTA_DISPARADO
FROM	[SRV-SQLSERVER-RECOMENDACAO].[Recomendacao-Data].[dbo].[cruzamentoMonitxVagaXCand] AS A	INNER JOIN [SRV-SQLSERVER-RECOMENDACAO].[Recomendacao-Data].[dbo].[cruzamentoMonit] AS B ON A.CodCzMonit_czMonitVagaCand = B.Cod_CzMonit
WHERE	A.CodVaga_czMonitVagaCand = @COD_VAGA
GROUP BY
		A.CodVaga_czMonitVagaCand ,
		CONVERT(SMALLDATETIME, CONVERT(VARCHAR, DataIni_czMonit, 112))
ORDER BY
		DATA_ENVIO ASC ;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
