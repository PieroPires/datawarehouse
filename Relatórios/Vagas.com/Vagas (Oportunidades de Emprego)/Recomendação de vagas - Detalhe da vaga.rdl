  <DataSets>
    <DataSet Name="DATASET_Recomendação_de_vagas">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cod_vaga">
            <Value>=Parameters!Cod_vaga.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Ident_cand">
            <Value>=Parameters!Ident_cand.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select	Ident_cand as [Identificação candidato]
		, Nome_cand as [Nome candidato]
		, isnull(convert(varchar, CodVaga_czMonitVagaCand), 'N/C') as [Código da vaga]
		, isnull(convert(char, max(DataIni_czMonit), 103), 'N/C') as [Data de envio] 
		, isnull(convert(char, DtCriacao_vaga, 103), 'N/C') as [Data criação vaga]
		, isnull(Cargo_vaga, 'N/C') as [Cargo da vaga]
		, isnull(Ident_cli, 'N/C') as [Identificação cliente]
		, isnull(Nome_cli, 'N/C') as [Nome cliente]
		, isnull(Descr_formMax, 'Indiferente') as [Escolaridade Mínima]
from	[hrh-data].[dbo].[Candidatos]	left outer join [hrh-data].[dbo].[cruzamentoMonitxVagaXCand]	on Cod_cand = CodCand_czMonitVagaCand
										and CodVaga_czMonitVagaCand = @Cod_vaga
										left outer join [hrh-data].[dbo].[cruzamentoMonit] on CodCzMonit_czMonitVagaCand = Cod_CzMonit
										left outer join [hrh-data].[dbo].[Vagas] on CodVaga_czMonitVagaCand = Cod_vaga 
										left outer join [hrh-data].[dbo].[Clientes] on CodCliente_vaga = Cod_cli
										left outer join [hrh-data].[dbo].[Cad_formacaoMax] on CodFormacaoMin_vaga = Cod_formMax
where	Ident_cand = @Ident_cand
group by Ident_cand, Nome_cand, CodVaga_czMonitVagaCand, DtCriacao_vaga, Cargo_vaga, Ident_cli, Nome_cli, Descr_formMax</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
