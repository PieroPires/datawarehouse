  <DataSets>
    <DataSet Name="DATASET_Ranking_Mensal_Analistas_RH">
      <Query>
        <DataSourceName>SQLMIRROR</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Data_limite_inferior">
            <Value>=Parameters!Data_limite_inferior.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Data_limite_superior">
            <Value>=Parameters!Data_limite_superior.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@data_Ranking">
            <Value>=Parameters!data_Ranking.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Cod_cli">
            <Value>=Parameters!Cod_cli.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH Respostas_PorVaga_novo as (
			select	vaga_id as CodVaga
					, COUNT( CASE WHEN tipo_avaliacao = 'descricao_empresa' then 1.00 else NULL end ) as QtdeRespostas_QualidadeEmpresa
					, AVG( CASE WHEN tipo_avaliacao = 'descricao_empresa' then 1.00 * avaliacao else NULL end ) as Media_QualidadeEmpresa

					, COUNT( CASE WHEN tipo_avaliacao = 'descricao_anuncio' then 1.00 else NULL end ) as QtdeRespostas_QualidadeAnuncio
					, AVG( CASE WHEN tipo_avaliacao = 'descricao_anuncio' then 1.00 * avaliacao else NULL end ) as Media_QualidadeAnuncio

					, COUNT( CASE WHEN tipo_avaliacao = 'processo_seletivo' then 1.00 else NULL end ) as QtdeRespostas_QualidadeProcesso
					, AVG( CASE WHEN tipo_avaliacao = 'processo_seletivo' and avaliacao &gt; 0 then 1.00 WHEN tipo_avaliacao = 'processo_seletivo' and avaliacao = 0 then 0.00 else NULL end ) as Media_QualidadeProcesso
			from	[vagas-data].[dbo].[avaliacoes_anuncios_de_vagas] with(nolock)
			where	created_at between @Data_limite_inferior and @Data_limite_superior
					AND avaliacao IS NOT NULL
			group by vaga_id
		), Respostas_Consolidado as (
			select	RN.CodVaga as CodVaga
					--transformando em escala de 0 a 1000 (Anuncio:  antigo 0..3, novo 1..5 - Atendimento Antigo 0..3, Novo 0..1 )
					, ISNULL(RN.QtdeRespostas_QualidadeEmpresa,0) + ISNULL(RN.QtdeRespostas_QualidadeAnuncio,0) as QtdeRespostas_QualidadeAnuncio
					, case when (ISNULL(RN.QtdeRespostas_QualidadeEmpresa,0) + ISNULL(RN.QtdeRespostas_QualidadeAnuncio,0)) &gt; 100 then 100 else (ISNULL(RN.QtdeRespostas_QualidadeEmpresa,0) + ISNULL(RN.QtdeRespostas_QualidadeAnuncio,0)) end as PesoQualidadeAnuncio
					,(	  floor( (  ISNULL(RN.Media_QualidadeEmpresa - 1 ,0) ) * 250) * ISNULL(RN.QtdeRespostas_QualidadeEmpresa,1)
						+ floor( (  ISNULL(RN.Media_QualidadeAnuncio - 1 ,0) ) * 250) * ISNULL(RN.QtdeRespostas_QualidadeAnuncio,1)
					 ) / IIF( ( ISNULL(RN.QtdeRespostas_QualidadeEmpresa,1) + ISNULL(RN.QtdeRespostas_QualidadeAnuncio,1) ) = 0, 1, ( ISNULL(RN.QtdeRespostas_QualidadeEmpresa,1) + ISNULL(RN.QtdeRespostas_QualidadeAnuncio,1) ) ) as QualidadeAnuncio
					, ( ISNULL(RN.QtdeRespostas_QualidadeProcesso,0)) as QtdeRespostas_QualidadeAtendimento
					, case when (ISNULL(RN.QtdeRespostas_QualidadeProcesso,0)) &gt; 100 then 100 else ( ISNULL(RN.QtdeRespostas_QualidadeProcesso,0)) end as Peso_qualidadeAtendimento	
					,(	floor( ( ISNULL(RN.Media_QualidadeProcesso,0)  ) * 1000) * ISNULL(RN.QtdeRespostas_QualidadeProcesso,1)	
					) / IIF( ISNULL(RN.QtdeRespostas_QualidadeProcesso,1)= 0, 1, ISNULL(RN.QtdeRespostas_QualidadeProcesso,1)) as QualidadeAtendimento
			from	Respostas_PorVaga_novo RN
		), AgrupadoFuncionario as (
			select	Cod_cli as codcli_agrupadoFunc
				, CodFunc_vaga as codFunc_agrupadoFunc
				, count(distinct cod_vaga) as QuantidadeVagasAbertas_agrupadoFunc
				, SUM( QtdeRespostas_QualidadeAtendimento ) as QtdeRespostas_QualidadeAtendimento
				, SUM( QtdeRespostas_QualidadeAnuncio ) as QtdeRespostas_QualidadeAnuncio

				, SUM( 1.00 * QualidadeAnuncio * PesoQualidadeAnuncio) / SUM( 1.00 * PesoQualidadeAnuncio) as QualidadeAnuncio_agrupadoFunc
				, SUM( 1.00 * QualidadeAtendimento * Peso_qualidadeAtendimento) / SUM( 1.00 * Peso_qualidadeAtendimento) as QualidadeAtendimento_agrupadoFunc

			from Respostas_Consolidado
			inner join [hrh-data].[dbo].[Vagas] with(NOLOCK) on codVaga = Cod_vaga
			inner join [hrh-data].[dbo].[Clientes] WITH(NOLOCK) on codcliente_vaga = Cod_cli
			group by Cod_cli, CodFunc_vaga
			having SUM(QtdeRespostas_QualidadeAnuncio) &gt;= 1
			AND SUM(QtdeRespostas_QualidadeAtendimento) &gt;= 1
		), prefinal as (
		select	@data_Ranking as Data_rank
				, codcli_agrupadoFunc as CodCli_rank	
				, codFunc_agrupadoFunc as codFunc_rank
				, QuantidadeVagasAbertas_agrupadoFunc as QuantidadeVagasAvaliadas_rank
				, QualidadeAnuncio_agrupadoFunc as QualidadeAnuncio_rank
				, QualidadeAtendimento_agrupadoFunc as QualidadeAtendimento_rank
				, (0.60 * QualidadeAtendimento_agrupadoFunc ) + ( 0.40 * QualidadeAnuncio_agrupadoFunc) as QualidadeGeral_rank
				, row_number() over ( partition by codcli_agrupadoFunc order by (0.60 * QualidadeAtendimento_agrupadoFunc ) + ( 0.40 * QualidadeAnuncio_agrupadoFunc) desc, QualidadeAtendimento_agrupadoFunc desc) as posicao_rank
				, QtdeRespostas_QualidadeAnuncio as QuantidadeRespostasAnuncio_rank
				, QtdeRespostas_QualidadeAtendimento as QuantidadeRespostasAtendimento_rank
		from	AgrupadoFuncionario
				left join [hrh-data].[dbo].[PesquisaQualidade_ranking]
				on codcli_rank = codcli_agrupadoFunc
				and data_rank = @data_Ranking
		)
		select	cod_cli, ident_cli, nome_cli, ranking_cli.posicao_rank as ranking_empresa, cod_func, nome_func, email_func, ranking_func.posicao_rank as ranking_funcionario, ranking_func.QuantidadeVagasAvaliadas_rank, ranking_func.QuantidadeRespostasAnuncio_rank, ranking_func.QualidadeAnuncio_rank, ranking_func.QuantidadeRespostasAtendimento_rank, ranking_func.QualidadeAtendimento_rank, ranking_func.QualidadeGeral_rank
		from	prefinal as ranking_func
				inner join [hrh-data].[dbo].[clientes] with(nolock) on codcli_rank = cod_cli 
				inner join [hrh-data].[dbo].[funcionarios] with(nolock) on codfunc_rank = cod_func
				inner join [hrh-data].[dbo].[PesquisaQualidade_ranking] as ranking_cli with(nolock) on ranking_cli.data_rank = ranking_func.Data_rank and ranking_cli.CodCli_rank = ranking_func.CodCli_rank
		where	ranking_func.codcli_rank = @Cod_cli or @Cod_cli is null
		order by ranking_cli.posicao_rank, ident_cli, ranking_func.posicao_rank</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="cod_cli">
          <DataField>cod_cli</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
