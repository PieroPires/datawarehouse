  <DataSets>
    <DataSet Name="DATASET_Fase_do_candidato_na_vaga">
      <Query>
        <DataSourceName>VAGASPROD</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cod_cand">
            <Value>=Parameters!Cod_cand.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CPF_cand">
            <Value>=Parameters!CPF_cand.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Cod_vaga">
            <Value>=Parameters!Cod_vaga.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select    CodCand_faseCand		as [Código Candidato]
 , ident_cand    			as [Identificação]
 , convert(datetime,Dt_faseCand,103)  as [Data alteração]
 , DtEntrada_faseCand 		as [Data Entrada na Fase]
 , DtSaida_faseCand   		as [Data Saída da Fase]
 , Nome_func      			as [Nome funcionário]   
 , Nome_fase      			as [Nome da Fase] 
 , case when(CodAval_faseCand =-1)    then 'Carinha Triste' 
when(CodAval_faseCand =0)    then 'Sem Carinha'          
when(CodAval_faseCand =0 and Eliminado_faseCand = 1) then 'Carinha Reta'           
when(CodAval_faseCand =2)    then 'Carinha Feliz'         
when(CodAval_faseCand =3)    then 'Carinha Super Feliz'      
else    null 
end as Carinha
 , case when Eliminado_faseCand = 1 then 'Sim'        
when Eliminado_faseCand = 0 then 'Não'   
else null 
end  as [Eliminado]  
from  [hrh-data].dbo.HistoricoFasesCand     with(nolock)  
 left join [hrh-data].dbo.Funcionarios  with(nolock)  
 on Cod_func = CodFunc_faseCand   
 inner join [hrh-data].dbo.Cad_fases     with(nolock)  
 on cod_fase = CodFase_faseCand  
 inner join [hrh-data].dbo.Candidatos     with(nolock)  
 on CodCand_faseCand = cod_cand  
where CodCand_faseCand    = isnull(@Cod_cand    , CodCand_faseCand)
 and cpf_cand    		 = isnull(@CPF_cand    , cpf_cand)  
 and CodVaga_faseCand    	 = @Cod_vaga
 and not (    @Cod_cand is null
             and @CPF_cand  is null 
 )     
order by [Data alteração], DtEntrada_faseCand, DtSaida_faseCand  </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
