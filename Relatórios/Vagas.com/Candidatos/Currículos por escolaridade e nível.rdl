  <DataSets>
    <DataSet Name="DATASET_Currículos_EscolaridadexNível">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Estado">
            <Value>=Parameters!Estado.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with cte_Nivel as(
	select	distinct CodCand_cargo
			,CodHierarquia_cargo
			from	[hrh-data].[dbo].[CandidatoxCargos]	with(nolock)
	where	CodHierarquia_cargo=22	--Auxiliar/Operacional
	union all

	select distinct	CodCand_cargo
			,CodHierarquia_cargo
	from	[hrh-data].[dbo].[CandidatoxCargos] with(nolock)
	where	CodHierarquia_cargo=30 --Júnior/Trainee
	union all

	select distinct	CodCand_cargo
			,CodHierarquia_cargo
	from	[hrh-data].[dbo].[CandidatoxCargos] with(nolock)
	where	CodHierarquia_cargo=40
	union all

	select distinct	CodCand_cargo
			,CodHierarquia_cargo
	from	[hrh-data].[dbo].[CandidatoxCargos]	with(nolock)
	where	CodHierarquia_cargo=50
)

select	sum(case when  Cod_hierarquia= 22	then 1 else 0 end) as [Auxiliar/Operacional]
		,sum(case when Cod_hierarquia= 30	then 1 else 0 end) as [Júnior/Trainee]
		,sum(case when Cod_hierarquia= 40	then 1 else 0 end) as [Pleno]
		,sum(case when Cod_hierarquia= 50	then 1 else 0 end) as [Sênior]
		--,Descr_hierarquia as Hierarquia
		,Descr_formMax	as Formacao
		,CodUF_cand	as CodUF_cand
		,count(*) as CVs
		into #NivxEscolaridade
from	cte_Nivel
