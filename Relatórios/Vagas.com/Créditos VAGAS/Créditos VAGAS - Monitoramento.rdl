    <DataSet Name="DataSet_Clientes">
      <Query>
        <DataSourceName>stage</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cliente">
            <Value>=Parameters!Cliente.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Cod_Cli,Ident_Cli FROM [HRH-DATA].DBO.Clientes WHERE Ident_Cli LIKE '%' + @Cliente + '%'
AND CreditosVAGAS_cli = 1
ORDER BY Ident_Cli</CommandText>
      </Query>
      <Fields>
        <Field Name="Cod_Cli">
          <DataField>Cod_Cli</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Nome_Cli">
          <DataField>Nome_Cli</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Ident_Cli">
          <DataField>Ident_Cli</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DATASET_SQLSERVER">
      <Query>
        <DataSourceName>stage</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@COD_CLI">
            <Value>=Parameters!lista_cliente.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  CASE WHEN CompraID_CredV IS NULL THEN 'Compra OFFLINE (via manut)' ELSE CONVERT(NVARCHAR(100), CompraID_CredV) END AS [Código da compra], 
        Ident_cli AS [Cliente],
        Credito_CredV AS [Qtde de Creditos VAGAS], 
        Data_CredV AS [Data do crédito], 
        DataVal_CredV AS [Data de validade do crédito],                 
        Custo_CredV AS [Custo], 
        Ident_func AS [ID do funcionário que efetuou a compra], -- CodFuncResp_CredV
        Nome_func AS [Nome do funcionário que efetuou a compra], -- CodFuncResp_CredV
        Nome_usuman AS [Manut - Usuário que lançou o crédito], -- CodUsuManut_CredV
        RespContrato_CredV AS [Manut - Responsável no cliente], 
        Obs_CredV AS [Manut - Observação], 
        Descr_CredVtipo AS [Descrição da operação]   
FROM [HRH-DATA].DBO.Creditos_Vagas A
INNER JOIN [HRH-DATA].DBO.Clientes B ON B.Cod_cli = A.CodCli_CredV
INNER JOIN [HRH-DATA].DBO.Creditos_vagas_tipoCredito C ON C.Cod_CredVtipo = A.TipoCredito_CredV
LEFT JOIN [HRH-DATA].DBO.Funcionarios D ON D.Cod_func = A.CodFuncResp_CredV
LEFT JOIN [HRH-DATA].DBO.ManutUsuarios E ON E.Cod_usuman = A.CodUsuManut_CredV
WHERE B.Cod_cli = @COD_CLI 
ORDER BY A.Data_CredV DESC</CommandText>
      </Query>
