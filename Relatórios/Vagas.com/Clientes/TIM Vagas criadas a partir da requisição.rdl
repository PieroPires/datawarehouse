  <DataSets>
    <DataSet Name="DATASET_TIM_Requisicoes_criadas">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cod_Vaga_SAP">
            <Value>=Parameters!Cod_Vaga_SAP.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 100 
	  TxtLim_resp			AS [Cód SAP]
	, Arquivo_TimReqVaga    AS [Arquivo]
	, Cod_rvw				AS [Cód da requisição]
	, Data_rvw				AS [Data da requisição]
	, CodVaga_rvw			AS [Cód da vaga] 
FROM [hrh-data].[dbo].[ZZZ_Cliente5511-Ficha285943-Resp] WITH(NOLOCK) 
	INNER JOIN [hrh-data].[dbo].[ReqVaga-Workflow]	   WITH(NOLOCK) 
	ON CodRespCabFic_rvw = CodCab_resp 
	INNER JOIN [hrh-data].[dbo].[TIM_Requisicao_Vaga]	   WITH(NOLOCK) 
	ON CodRVW_TimReqVaga = Cod_rvw 
WHERE CodPerg_resp = 1042445 
	AND TxtLim_resp LIKE '%' + isnull(@Cod_Vaga_SAP, TxtLim_resp) + '%' 
	ORDER BY Cod_rvw DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Cód_SAP">
          <DataField>Cód SAP</DataField>
