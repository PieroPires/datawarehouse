  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>VAGAS_PROD</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cod_cand">
            <Value>=Parameters!Cod_cand.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Cod_vaga">
            <Value>=Parameters!Cod_vaga.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DtInicio">
            <Value>=Parameters!DtInicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DtFim">
            <Value>=Parameters!DtFim.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select CodVaga_arqExp as [Código da vaga]
		, CodCand_arqExp as [Código do candidato]
		, NomeArquivo_arqExp as [Nome do arquivo]
		, DtEnvio_arqExp as [Data de envio]
from	[hrh-data].[dbo].[WhiteMartins_Arquivo_Exportacao]	with(nolock)
where	CodCand_arqExp		= @Cod_cand	
		and CodVaga_arqExp	= isnull(@Cod_vaga, CodVaga_arqExp)	
		and DtEnvio_arqExp between isnull(@DtInicio, DtEnvio_arqExp) and isnull(@DtFim, DtEnvio_arqExp) ;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
