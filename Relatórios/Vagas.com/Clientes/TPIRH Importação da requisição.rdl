  <DataSets>
    <DataSet Name="DATASET_TPIRH_Importacao_de_requisicao">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Codigo_CRM">
            <Value>=Parameters!Codigo_CRM.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 100 
	  TxtLim_resp   AS [Código CRM]			
	, Cod_rvw       AS [Código da requisição]	
	, Data_rvw		AS [Data da requisição]	
	, CodVaga_rvw	AS [Código da vaga]			
FROM [hrh-data].[dbo].[ZZZ_Cliente33833-Ficha278445-Resp]			
WITH(NOLOCK) 
	INNER JOIN [hrh-data].[dbo].[ReqVaga-Workflow]					WITH(NOLOCK) 
	ON CodRespCabFic_rvw = CodCab_resp 
WHERE CodPerg_resp = 997585 
	  AND TxtLim_resp LIKE '%' + isnull(@Codigo_CRM, TxtLim_resp) + '%' 
ORDER BY Cod_rvw DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Código_CRM">
          <DataField>Código CRM</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Código_da_requisição">
          <DataField>Código da requisição</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Data_da_requisição">
          <DataField>Data da requisição</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Código_da_vaga">
          <DataField>Código da vaga</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
