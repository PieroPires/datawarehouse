  <DataSets>
    <DataSet Name="DATASET_Boticario_Status_Geral">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Codigo_da_vaga">
            <Value>=Parameters!Codigo_da_vaga.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Codigo_SP">
            <Value>=Parameters!Codigo_SP.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Nome_candidato">
            <Value>=Parameters!Nome_candidato.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Mais_recente">
            <Value>=Parameters!Mais_recente.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TOP 100
      DataInfo_BotSolPes	AS 'Data'
    , NumSP_BotSolPes	AS 'SP'  
    , CodVaga_BotSolPes	AS 'Vaga'
    , Cargo_BotSolPes	AS 'Cargo'
    , Cod_cand		AS 'Código do Candidato'    
    , Nome_cand		AS 'Candidato'  
    , CPF_cand		AS 'CPF do Candidato'
    , CASE WHEN Eliminado_candVaga = 0		THEN 'Não' ELSE 'Sim' END AS 'Eliminado'
    , CASE WHEN CodFase_candVaga = 461589	THEN 'Sim' ELSE 'Não' END AS 'Fase de Movimentação'
    , CASE WHEN FicMov.Cod_respCab IS NULL	THEN 'Não' ELSE 'Sim' END AS 'Ficha de Movimentação Preenchida'
    , CASE WHEN CodFase_candVaga = 461590	THEN 'Sim' ELSE 'Não' END AS 'Fase de Admissão'
    , CASE WHEN FicAdm.Cod_respCab IS NULL	THEN 'Não' ELSE 'Sim' END AS 'Ficha de Admissão Preenchida'
    , CASE WHEN FicInfo.Cod_respCab IS NULL THEN 'Não' ELSE 'Sim' END AS 'Ficha de Informações Complementares Preenchida'
    , CASE WHEN Data_BotExp IS NULL THEN 'Não' ELSE CONVERT(VARCHAR, Data_BotExp, 120) END AS 'Exportado'
    , CASE WHEN DtUltAtualizacao_BotNExp IS NULL THEN '' ELSE RetornoSAP_BotNExp END AS 'Erro do SAP'
FROM [hrh-data].[dbo].[Boticario_SolicitPessoal_SAP]						WITH(NOLOCK)
	LEFT JOIN [hrh-data].[dbo].[Candidatos]								WITH(NOLOCK) 
	ON Cod_cand = CodCand_BotSolPes
	LEFT JOIN [hrh-data].[dbo].[CandidatoxVagas]							WITH(NOLOCK) 
	ON CodVaga_BotSolPes = CodVaga_candVaga 
	AND Cod_cand = CodCand_candVaga
	LEFT JOIN [hrh-data].[dbo].[Boticario_Candidatos_Exportados]			WITH(NOLOCK) 
	ON NumSP_BotSolPes = NumSP_botExp 
	AND CodCand_candVaga = CodCand_botExp 
	AND CodVaga_candVaga = CodVaga_botExp
	LEFT JOIN [hrh-data].[dbo].[Boticario_Candidatos_Nao_Exportados]		WITH(NOLOCK) 
	ON CONVERT(INT, NumSP_BotSolPes) = NumSP_botNExp 
	AND CodCand_candVaga = CodCand_botNExp
	LEFT JOIN [hrh-data].[dbo].[ZZZ_Cliente39684-Fichas-RespCab] AS FicMov WITH(NOLOCK) 
	ON FicMov.CodCand_respCab = CodCand_candVaga 
	AND FicMov.CodFicha_respCab = 266336
	LEFT JOIN [hrh-data].[dbo].[ZZZ_Cliente39684-Fichas-RespCab] AS FicAdm WITH(NOLOCK) 
	ON FicAdm.CodCand_respCab = CodCand_candVaga 
	AND FicAdm.CodVaga_respCab = CodVaga_candVaga 
	AND FicAdm.CodFicha_respCab = 265325
	LEFT JOIN [hrh-data].[dbo].[ZZZ_Cliente39684-Fichas-RespCab] AS FicInfo WITH(NOLOCK) 
	ON FicInfo.CodCand_respCab = CodCand_candVaga 
	AND FicInfo.CodFicha_respCab = 265322
WHERE (@Codigo_da_vaga IS NULL OR CodVaga_BotSolPes = @Codigo_da_vaga)
  AND NumSP_BotSolPes = ISNULL(@Codigo_SP, NumSP_BotSolPes)  
  AND (@Nome_candidato IS NULL OR Nome_cand LIKE '%' + @Nome_candidato + '%')  
  AND DataInfo_BotSolPes &gt;= ISNULL(@Mais_recente, DataInfo_BotSolPes)  
ORDER BY data DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Data">
          <DataField>Data</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
