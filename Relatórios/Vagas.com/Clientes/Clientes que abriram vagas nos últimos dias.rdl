  <DataSets>
    <DataSet Name="DATASET_Clientes_abriram_vagas">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Dias">
            <Value>=Parameters!Dias.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DtInicio">
            <Value>=Parameters!DtInicio.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with VagasAbertas as(
select	distinct Cod_vaga	as Cod_vaga
		, Nome_fase	as Nome_fase
		, Cod_cli	as Cod_cli
		, Ident_cli as Ident_cli
		, DtCriacao_vaga as DtCriacao_vaga
		, DtExpiracaoWeb_vaga as DtExpiracaoWeb_vaga		
from	[hrh-data].[dbo].[clientes]		inner join [hrh-data].[dbo].[vagas] on cod_cli = CodCliente_vaga
										inner join [hrh-data].[dbo].[CandidatoxVagas] on CodVaga_candVaga = cod_vaga
										inner join [hrh-data].[dbo].[Cad_fases] on Cod_fase  = CodUltFaseCand_vaga 
where	Bloqueado_cli= 0 --Cliente não está bloqueado
		and Ficticio_cli= 0 --Cliente não é fictício
		and VagaModelo_vaga= 0 --Vaga não é modelo
		and Validada_vaga= 1 --Vaga foi validada
		and ColetaCur_vaga= 0 --Vaga não é de captação
		and CadastroValidado_cli= 1 --Cadastro do cliente foi validado
		and DivulgacaoLiberada_vaga= 1 --Vaga com divulgação liberada
		and Teste_vaga= 0 --Vaga não é teste
		and left(Cargo_vaga, 5) not like '%demo[ -]%' 
		and DtExpiracaoWeb_vaga &gt;= dateadd(day, 1, getdate())
		and cast(DtCriacao_vaga as date) &gt;= dateadd(day, cast(@Dias as smallint) , @DtInicio) 
)
select	Ident_cli	as [Cliente]
		, count(Cod_vaga) as [Vagas]
		, sum(case when Nome_fase like 'Candidatura%' then 0 else 1 end)  as [Acima da candidatura]
		, sum(case when Nome_fase like 'Candidatura%' then 1 else 0 end) as [Na candidatura]
		, max(DtCriacao_vaga) as [Última data de criação]
		, max(DtExpiracaoWeb_vaga) as [Última data de expiração]
from	VagasAbertas 
group by
		Ident_cli
order by
		[Última data de criação] ;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
