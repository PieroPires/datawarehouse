        </Field>
        <Field Name="QtdExlusivaEmpresa">
          <DataField>QtdExlusivaEmpresa</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="QtdVagasEmConjuntoComEmpresa">
