        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DATASET_Mes">
      <Query>
        <DataSourceName>SQLBI01</DataSourceName>
        <CommandText>select convert(tinyint,'1') as id, 'Janeiro' as Mes
union
select convert(tinyint,'2'), 'Fevereiro'
union
select convert(tinyint,'3'), 'Março'
union
select convert(tinyint,'4'), 'Abril'
union
select convert(tinyint,'5'), 'Maio'
union
select convert(tinyint,'6'), 'Junho'
union
select convert(tinyint,'7'), 'Julho'
union
select convert(tinyint,'8'), 'Agosto'
union
select convert(tinyint,'9'), 'Setembro'
union
select convert(tinyint,'10'), 'Outubro'
union
select convert(tinyint,'11'), 'Novembro'
union
select convert(tinyint,'12'), 'Dezembro'
order by id;</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="Mes">
          <DataField>Mes</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DATASET_Ano">
      <Query>
        <DataSourceName>SQLBI01</DataSourceName>
        <CommandText>select	distinct datepart(year, Data) as Ano
from	[Relatorio].[dbo].[ResponsabilidadeSocial]
order by	Ano;</CommandText>
      </Query>
      <Fields>
        <Field Name="Ano">
          <DataField>Ano</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
