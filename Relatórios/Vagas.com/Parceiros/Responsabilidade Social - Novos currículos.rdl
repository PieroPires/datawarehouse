  <DataSets>
    <DataSet Name="DATASET_Novos_curriculos">
      <Query>
        <DataSourceName>SQLBI01</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Mes">
            <Value>=Parameters!Mes.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Ano">
            <Value>=Parameters!Ano.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     Data, Ident_cli, Nome_cli, AnoMes, QtdExlusivaEmpresa, QtdVagasEmConjuntoComEmpresa, QtdePosicoes_vaga
FROM      [Relatorio].[dbo].[ResponsabilidadeSocial]
WHERE    MONTH(Data)=@Mes
	 AND YEAR(Data)=@Ano; </CommandText>
      </Query>
      <Fields>
        <Field Name="Data">
          <DataField>Data</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Ident_cli">
          <DataField>Ident_cli</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Nome_cli">
          <DataField>Nome_cli</DataField>
