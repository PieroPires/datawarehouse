    <DataSet Name="DATASET_FirstDay_LastDay_Month">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <CommandText>select	convert(datetime, cast(dateadd(day, datepart(day, eomonth(getdate())) * -1, dateadd(day, 1, eomonth(getdate()))) as char(10)) + ' ' + '00:00:00')  as Primeiro_dia_Mes
		, convert(datetime, cast(eomonth(getdate()) as char(10)) + ' ' + '23:59:59') as Ultimo_dia_Mes</CommandText>
      </Query>
      <Fields>
        <Field Name="Primeiro_dia_Mes">
          <DataField>Primeiro_dia_Mes</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Ultimo_dia_Mes">
          <DataField>Ultimo_dia_Mes</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
