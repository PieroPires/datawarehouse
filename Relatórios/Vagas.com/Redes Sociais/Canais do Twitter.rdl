  <DataSets>
    <DataSet Name="DATASET_Canais_Twitter">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Dt_Inicio">
            <Value>=Parameters!Dt_Inicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dt_Final">
            <Value>=Parameters!Dt_Final.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with	Cont_Twitter(canal_vagTwit, codvaga_vagTwit) as(
	select	canal_vagTwit
			, codvaga_vagTwit
	from	[hrh-data].[dbo].[Vagas-twitter] with(nolock)	inner join	[hrh-data].[dbo].[clientes-twitter]  with(nolock) on canal_vagTwit = ScreenName_cliTwit
	where	publicada_vagTwit &gt;= @Dt_Inicio and publicada_vagTwit &lt;= @Dt_Final
			and Status_cliTwit = 1
			and CodFuncExclusao_cliTwit is null
), Cont_Candidaturas(codvaga_candvaga, codcand_candvaga, DtPrimCand_histCand) as (
	select	codvaga_candvaga, codcand_candvaga, DtPrimCand_histCand
	from	[hrh-data].[dbo].[CandidatoxVagas]  with(nolock)
			inner join [hrh-data].[dbo].[historicoCandidaturas] with(nolock)
			on codcand_histcand = CodCand_candVaga and CodVaga_histCand = CodVaga_candVaga
	where	FonteTipo_CandVaga = 9
			--and FonteCompl_candVaga is not null
), PrimeiraCandidatura ( CodCand_PriCand, Dt_priCand ) as (
	select	CodCand_histCand, min(DtPrimCand_histCand) as PrimeiraCandidatura
	from	[hrh-data].[dbo].[historicoCandidaturas] with(nolock)
	group by CodCand_histCand
)
select	canal_vagTwit as Canal_publicacao
		, count(distinct codvaga_vagTwit) as Publicacoes
		, count(CodCand_candVaga) as Candidaturas
		, count(case when DtPrimCand_histCand = Dt_priCand then 1 else null end) as NovosCVs
from	Cont_Twitter	
		left outer join	Cont_Candidaturas  with(nolock) on codvaga_vagTwit = codvaga_candvaga
		left join PrimeiraCandidatura on codcand_candvaga = CodCand_PriCand
group by canal_vagTwit 
order by canal_vagTwit;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
