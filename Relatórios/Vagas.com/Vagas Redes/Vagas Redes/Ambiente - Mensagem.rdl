        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Posicoes_Solicitadas">
          <DataField>Total Posicoes Solicitadas</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total_Candidaturas">
          <DataField>Total Candidaturas</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total_Cvs_Distintos_Visualisados_pela_Vaga_Unidade">
          <DataField>Total Cvs Distintos Visualisados pela Vaga/Unidade</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total_Carinhas_Aplicadas">
          <DataField>Total Carinhas Aplicadas</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total_de_Candidatos_Contratados">
          <DataField>Total de Candidatos Contratados</DataField>
