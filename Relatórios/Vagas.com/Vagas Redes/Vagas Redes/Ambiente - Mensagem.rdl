    <DataSet Name="DATASET_Ambiente_Unidade">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cod_cli">
            <Value>=Parameters!Cod_cli.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DtInicio">
            <Value>=Parameters!DtInicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DtFim">
            <Value>=Parameters!DtFim.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-------------------------------------------------------------------------------------------------------------
-- Magazine 
-- Aba: Ambiente Unidade        
-------------------------------------------------------------------------------------------------------------
;
with  VagasRedes as	(
		select   cod_vaga 
				, ident_cli 
				, cod_cli
				, cargo_vaga 
				, dtCriacao_Vaga  
				, dtExpiracaoWeb_Vaga
				, case 
						when dtExpiracaoWeb_Vaga &gt;= getdate() then 'Aberta' 
						else 'Expirada'
					end as 'Status'
				, QtdePosicoes_vaga
				, codfunc_vaga	
		from	[hrh-data].[dbo].[Vagas]	inner join [hrh-data].dbo.FranqueadorxFranqueado on CodFranqueado_vaga = Cod_fcf 
											left outer join [hrh-data].dbo.Clientes on CodCliFranqueado_fcf = cod_cli
											left outer join [hrh-data].dbo.Funcionarios on codfunc_vaga = cod_func 
		where	codcliFranqueador_fcf = @Cod_cli
				and ColetaCur_vaga = 0 -- Não contar vagas de CAPTAÇÃO
				and TipoProcesso_vaga = 4 --in  (0, 1, 2) -- 0 - Normal / 1 - PET / 2 - PRC / 4 - REDES
				and Teste_vaga = 0 -- -- nao eh uma vaga de teste (Não considerar no Cálculo)
				and VagaModelo_vaga = 0 -- Vaga não é modelo            
				and left(Cargo_vaga,5) not in ('demo ', 'demo-') -- a vaga nao foi criada durante uma demonstracao  
				and Validada_vaga =  1 
				and ident_cli not in ('ficticia redes', 'vagasredes')
				-- and dtCriacao_Vaga between DATAINICIAL amd DATAFINAL

), Candidatura as (
      select    codVaga_histCand
				, count(*) as qtdCandidatura
      from		[hrh-data].[dbo].[HistoricoCandidaturas] inner join VagasRedes on cod_vaga = codVaga_histCand
      group by codVaga_histCand

), FonteCandidatura as (
      select   cod_vaga as codvaga_FonteCandidatura
			   , case
					when Descr_fonteCandidatura in	(
													'Novo sistema de email de monitoração' -- Alerta de Vagas 
													, 'Recomendacao de vagas por similaridade entre visualizações' -- Alerta ou Recomendação
													, 'Recomendação de vagas via vagas similares' -- Alerta ou Recomendação
													, 'Recomendação pós candidatura' -- Alerta ou Recomendação
													, 'Sistema de recomendação na página de pesquisa de vagas' -- Alerta ou Recomendação
													) then 'Alerta Vagas.com'
					else Descr_fonteCandidatura
				end as Descr_fonteCandidatura

			   , count(*)  as qtdFonteCandidatura
      from     [hrh-data].[dbo].[candidatoxvagas]	inner join VagasRedes on cod_vaga = CodVaga_candVaga
													inner join [hrh-data].[dbo].[Cad_FonteCandidaturas] on FonteTipo_candVaga = cod_fonteCandidatura
	  where	   Descr_fonteCandidatura in	(
											'Navegação normal pelo site (inclui pesquisa de vagas antiga)'
											, 'Novo sistema de email de monitoração' -- Alerta de Vagas 
											, 'Recomendacao de vagas por similaridade entre visualizações' -- Alerta ou Recomendação
											, 'Recomendação de vagas via vagas similares' -- Alerta ou Recomendação
											, 'Recomendação pós candidatura' -- Alerta ou Recomendação
											, 'Sistema de recomendação na página de pesquisa de vagas' -- Alerta ou Recomendação
											, 'Portal de carreira do VAGAS Redes'
											, 'Alerta de vagas do VAGAS Redes'

											)			
      group by cod_vaga
			   , case
					when Descr_fonteCandidatura in	(
													'Novo sistema de email de monitoração' -- Alerta de Vagas 
													, 'Recomendacao de vagas por similaridade entre visualizações' -- Alerta ou Recomendação
													, 'Recomendação de vagas via vagas similares' -- Alerta ou Recomendação
													, 'Recomendação pós candidatura' -- Alerta ou Recomendação
													, 'Sistema de recomendação na página de pesquisa de vagas' -- Alerta ou Recomendação
													) then 'Alerta Vagas.com'
					else Descr_fonteCandidatura
				end

), CurriculoVisto as (
      select   codVaga_cvVisto
				, count(distinct CodCand_cvVisto) as qtdCVVisto
      from     [hrh-data].[dbo].[Curriculo_Visto] inner join VagasRedes on cod_vaga = codVaga_cvVisto
      group by codVaga_cvVisto

), Carinha as (
      select   codVaga_faseCand
			 , count(distinct codcand_fasecand) as qtdCarinha
      from    [hrh-data].dbo.HistoricoFasesCand inner join VagasRedes on cod_vaga = codVaga_faseCand
      where    codAval_faseCand &lt;&gt; 0
      group by codVaga_faseCand

), FaseConcluido as (
      select   cod_fase  
      from     [hrh-data].[dbo].[Cad_fases] 
      where    codcli_fase in (select distinct cod_cli from VagasRedes) 
               and nome_fase = 'Concluído'

), Concluido as (
      select   codVaga_faseCand
			 , count(distinct codcand_fasecand)  as qtdConcluido
      from     [hrh-data].[dbo].[HistoricoFasesCand]	inner join VagasRedes on cod_vaga = codVaga_faseCand
														inner join FaseConcluido on cod_fase = CodFase_faseCand 
      group by codVaga_faseCand
)

-- Ambiente Unidade
select   (select ident_cli from [hrh-data].[dbo].[Clientes] where cod_cli = @Cod_cli) idMatriz --Combo
         , cod_vaga		as Vaga
         , ident_cli	as idUnidade
         , cargo_vaga	as Cargo
         , dtCriacao_Vaga as Abertura 
         , Status
         , QtdePosicoes_vaga 'Total Posicoes Solicitadas'  
         , isnull(qtdCandidatura, 0) 'Total Candidaturas'
         , isnull(qtdCVVisto, 0) 'Total Cvs Distintos Visualisados pela Vaga/Unidade'
         , isnull(qtdCarinha, 0) 'Total Carinhas Aplicadas'
         , isnull(qtdConcluido, 0) 'Total de Candidatos Contratados'
		 , isnull(FT1.qtdFonteCandidatura, 0) 'Total de Candidaturas via Vagas.com'
		 , isnull(FT2.qtdFonteCandidatura, 0) 'Total de Candidaturas via Alerta Vagas.com'
		 , isnull(FT3.qtdFonteCandidatura, 0) 'Total de Candidaturas via Alerta Redes'
		 , isnull(FT4.qtdFonteCandidatura, 0) 'Total de Candidaturas via Portal Redes'
from     VagasRedes		left outer join  Candidatura on cod_vaga = codVaga_histCand  
						left outer join  CurriculoVisto on cod_vaga = codVaga_cvVisto
						left outer join  Carinha on cod_vaga = Carinha.codVaga_faseCand 
						left outer join  Concluido on cod_vaga = Concluido.codVaga_faseCand 
						left outer join  FonteCandidatura FT1	on cod_vaga = FT1.codvaga_FonteCandidatura 
																and FT1.Descr_fonteCandidatura = 'Navegação normal pelo site (inclui pesquisa de vagas antiga)' 
						left outer join  FonteCandidatura FT2	on cod_vaga = FT2.codvaga_FonteCandidatura 
																and FT2.Descr_fonteCandidatura = 'Alerta Vagas.com'
						left outer join  FonteCandidatura FT3	on cod_vaga = FT3.codvaga_FonteCandidatura 
																and FT3.Descr_fonteCandidatura = 'Portal de carreira do VAGAS Redes'
						left outer join  FonteCandidatura FT4	on cod_vaga = FT4.codvaga_FonteCandidatura 
																and FT4.Descr_fonteCandidatura = 'Alerta de vagas do VAGAS Redes'
where	dtCriacao_Vaga between @DtInicio and cast(cast(@DtFim as date) as char) + '23:59:59'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="idMatriz">
          <DataField>idMatriz</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Vaga">
          <DataField>Vaga</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="idUnidade">
          <DataField>idUnidade</DataField>
