    <DataSet Name="DATASET_Mensagem">
      <Query>
        <DataSourceName>SQLMirror</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Cod_cli">
            <Value>=Parameters!Cod_cli.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DtInicio">
            <Value>=Parameters!DtInicio.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DtFim">
            <Value>=Parameters!DtFim.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with	VagasRedes as (
	select	cod_vaga, CodFranqueado_vaga, Codcliente_vaga 
	from	[hrh-data].[dbo].[Vagas]	inner join [hrh-data].[dbo].[FranqueadorxFranqueado] on CodFranqueado_vaga = Cod_fcf 
										left outer join [hrh-data].[dbo].[Clientes] on CodCliFranqueado_fcf = cod_cli
	where	codcliFranqueador_fcf = @Cod_cli
			and ColetaCur_vaga = 0 -- Não contar vagas de CAPTAÇÃO
			and TipoProcesso_vaga = 4 --in  (0, 1, 2) -- 0 - Normal / 1 - PET / 2 - PRC / 4 - REDES
			and Teste_vaga = 0 -- -- nao eh uma vaga de teste (Não considerar no Cálculo)
			and VagaModelo_vaga = 0 -- Vaga não é modelo            
			and left(Cargo_vaga, 5) not in ('demo ', 'demo-') -- a vaga nao foi criada durante uma demonstracao  
			and Validada_vaga =  1 
			and ident_cli not in ('ficticia redes', 'vagasredes')
			-- and dtCriacao_Vaga between DATAINICIAL amd DATAFINAL
)

SELECT	dt_hist AS Data, Ident_cli as Unidade, Codvaga_hist AS CodVaga, CodCand_hist AS CodCand, ident_func AS Usuario
		, Email_func AS EmailUsuario, replace(convert(varchar(2000),Anotacao_hist), char(13) + char(10), ' ' ) AS Mensagem
FROM	[hrh-data].[dbo].[Historico]	INNER JOIN VagasRedes on (Cod_vaga = Codvaga_hist and CodFranqueado_vaga is not null)
										INNER JOIN [hrh-data].[dbo].[Funcionarios] on (Cod_func = autor_hist)
										INNER JOIN [hrh-data].[dbo].[FranqueadorxFranqueado] on (Cod_fcf = CodFranqueado_vaga)
										INNER JOIN [hrh-data].[dbo].[Clientes] on (CodCliFranqueado_fcf = Cod_cli)
WHERE	Tipo_hist = -107 
		and Codcliente_vaga = @Cod_cli
		and dt_hist between @DtInicio and cast(cast(@DtFim as date) as char) + '23:59:59'
ORDER BY dt_hist</CommandText>
      </Query>
