          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DATASET_Ultimo_dia_mes_atual">
      <Query>
        <DataSourceName>OTRS</DataSourceName>
        <CommandText>select date_add(date_add(last_day(now()), interval 1 day), interval -1 second) as 'UltDia_mes_Atual';</CommandText>
