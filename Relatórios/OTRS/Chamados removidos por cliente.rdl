    <DataSet Name="DATASET_Primeiro_dia_mes_atual">
      <Query>
        <DataSourceName>OTRS</DataSourceName>
        <CommandText>select cast(cast(date_add(now(), interval (day(date_add(now(), interval -1 day)) * -1) day) as date) as datetime) as 'PrimDia_mes_Atual';</CommandText>
      </Query>
      <Fields>
        <Field Name="PrimDia_mes_Atual">
