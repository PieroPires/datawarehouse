  <DataSets>
    <DataSet Name="DATASET_Mensagens_Padrões_enviadas_por_cliente">
      <Query>
        <DataSourceName>OTRS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter2.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select	ti.tn as 'Protocolo'
		, st.name as 'Mensagem'
		, th.change_time as 'Data de Envio'
		, dfv.value_text as 'Página de Origem'
from	otrs.ticket as ti	inner join  otrs.ticket_history as th		on ti.id = th.ticket_id
							inner join	otrs.dynamic_field_value as dfv	on dfv.object_id = ti.id
							inner join	otrs.queue as qu				on qu.id = ti.queue_id
							inner join	otrs.queue_standard_template as qst	on qu.id = qst.queue_id
							inner join	otrs.standard_template as st	on st.id =	qst.standard_template_id
where	cast(th.change_time as date) between ? and ?
		and dfv.field_id=5		#Registro de valores da página de origem
		and	st.id= LEFT(TRIM(LEADING 'ResponseTemplate (' FROM th.name), LOCATE('/',TRIM(LEADING 'ResponseTemplate(' FROM th.name))-1) 
#pega o código da mensagem padrão que corresponde ao primeiro número do ResponseTemplate
order by th.change_time ;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Protocolo">
          <DataField>Protocolo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Mensagem">
          <DataField>Mensagem</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Data_de_Envio">
          <DataField>Data de Envio</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Página_de_Origem">
          <DataField>Página de Origem</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
