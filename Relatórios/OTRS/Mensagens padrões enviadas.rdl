  <DataSets>
    <DataSet Name="DATASET_Mensagens_padroes_enviadas">
      <Query>
        <DataSourceName>OTRS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter2.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select	ti.tn	as	'Protocolo'	
		,qu.name		as 'Fila'	
		,qu.id			as 'Cod_fila'			
		,st.name		as 'Mensagem Padrao'	
        ,th.change_time	as 'Data_Envio'	
from	otrs.standard_template	as st		
		inner join 	otrs.queue_standard_template as qst	on	st.id = qst.standard_template_id
		inner join	otrs.queue	as qu	on	qu.id = qst.queue_id
		inner join 	otrs.ticket	as ti 	on	qu.id = ti.queue_id
		inner join	otrs.ticket_history	as	th	on	ti.id = th.ticket_id
where	cast(th.change_time as date) 	between ? and ?		and st.id= LEFT(TRIM(LEADING 'ResponseTemplate (' FROM th.name), LOCATE('/',TRIM(LEADING 'ResponseTemplate(' FROM th.name))-1)
order by th.change_time, ti.tn;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
