        <CommandText>select	distinct qu.id	as Cod_fila
		,qu.name	as Fila
from	queue		as qu
		inner join	ticket	as ti	on qu.id = ti.queue_id
        inner join	queue_standard_template	as qst	on qu.id = qst.queue_id
		inner join	standard_template		as st	on qst.queue_id = qu.id
        inner join	ticket_history			as th	on ti.id = th.ticket_id
where	st.id = left(trim(leading 'ResponseTemplate (' from th.name), locate('/', trim(leading 'ResponseTemplate (' from th.name))-1)
		and cast(th.change_time as date) between ? and ?
order by qu.name;</CommandText>
      </Query>
      <Fields>
        <Field Name="Cod_fila">
          <DataField>Cod_fila</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Fila">
          <DataField>Fila</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DATASET_Filas_MensagensPadroes">
      <Query>
        <DataSourceName>OTRS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter1.Value</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!Parameter2.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select	qu.name	as Fila
		,count(*)	as Total
from	otrs.standard_template	as st		
		inner join 	otrs.queue_standard_template	as qst	on st.id = qst.standard_template_id
		inner join	otrs.queue	as qu	on	qu.id = qst.queue_id
		inner join 	otrs.ticket	as ti	on	qu.id = ti.queue_id
		inner join	otrs.ticket_history	as th	on	ti.id = th.ticket_id
where	cast(th.change_time as date) between ? AND ?
		and	st.id= LEFT(TRIM(LEADING 'ResponseTemplate (' FROM th.name), LOCATE('/',TRIM(LEADING 'ResponseTemplate(' FROM th.name))-1)
group by	qu.name
order by	Total desc;</CommandText>
      </Query>
      <Fields>
        <Field Name="Fila">
          <DataField>Fila</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
