        </Field>
        <Field Name="DATA_ENVIO">
          <DataField>DATA_ENVIO</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>OTRS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText># Query:
SELECT	NUMERO_CHAMADO ,
		FILA ,
        CORPO_MENSAGEM ,
        ASSUNTO ,
        MENSAGEM_ENVIADA ,
        DATA_ENVIO    
FROM	(
	SELECT	DISTINCT  CONCAT('"', A.tn, '"')	AS NUMERO_CHAMADO ,
			B.name	AS 'FILA' ,
			(SELECT	SUBSTRING(A1.a_body, LOCATE('Mensagem:', A1.a_body), LENGTH(A1.a_body) + LOCATE('Mensagem:', A1.a_body) * -1 + 1) AS CORPO_MENSAGEM
			 FROM	otrs.article AS A1
			 WHERE	A.id = A1.ticket_id
					AND A1.change_by = 1 -- Alteração admin otrs
			 ORDER BY A1.create_time ASC
			 LIMIT 1) 
					AS CORPO_MENSAGEM ,
			(SELECT	A1.a_subject AS ASSUNTO
			 FROM	otrs.article AS A1
			 WHERE	A.id = A1.ticket_id
					AND A1.change_by = 1 -- Alteração admin otrs
			 ORDER BY A1.create_time ASC
			 LIMIT 1) 
					AS ASSUNTO ,
			(SELECT	A2.name AS MENSAGEM_ENVIADA
			 FROM	otrs.ticket_history AS A1	LEFT OUTER JOIN standard_template AS A2 ON A2.id = LEFT(TRIM(LEADING 'ResponseTemplate (' FROM A1.name), LOCATE('/', TRIM(LEADING 'ResponseTemplate (' FROM A1.name))-1)
			 WHERE	A.id = A1.ticket_id
					AND A2.id = LEFT(TRIM(LEADING 'ResponseTemplate (' FROM A1.name), LOCATE('/', TRIM(LEADING 'ResponseTemplate (' FROM A1.name))-1)
					AND A1.create_time &gt;= '2017-03-01' AND A1.create_time &lt;= '2017-04-30 23:59:59'
			 ORDER BY A1.create_time ASC
			 LIMIT 1) AS MENSAGEM_ENVIADA ,
			 (SELECT A1.create_time AS DATA_ENVIO -- Data de envio da 1ª mensagem enviada.
			  FROM	otrs.ticket_history AS A1	LEFT OUTER JOIN standard_template AS A2 ON A2.id = LEFT(TRIM(LEADING 'ResponseTemplate (' FROM A1.name), LOCATE('/', TRIM(LEADING 'ResponseTemplate (' FROM A1.name))-1)
			  WHERE	A.id = A1.ticket_id
					AND A1.create_time &gt;= '2017-03-01' AND A1.create_time &lt;= '2017-04-30 23:59:59'
					AND A1.change_by != 1 -- Alteração não foi feita pelo admin OTRS (garantir data de envio)
					AND A2.id = LEFT(TRIM(LEADING 'ResponseTemplate (' FROM A1.name), LOCATE('/', TRIM(LEADING 'ResponseTemplate (' FROM A1.name))-1)
			 ORDER BY A1.create_time ASC
			 LIMIT 1) AS DATA_ENVIO
	FROM	otrs.ticket AS A		INNER JOIN otrs.queue AS B ON A.queue_id = B.id
									INNER JOIN otrs.ticket_history AS C ON A.id = C.ticket_id
									INNER JOIN otrs.standard_template AS D ON D.id = LEFT(TRIM(LEADING 'ResponseTemplate (' FROM C.name), LOCATE('/', TRIM(LEADING 'ResponseTemplate (' FROM C.name))-1) -- Retorna apenas as mensagens especificadas.
	WHERE	B.name IN ('FAQ VAGAS') 
			AND C.create_time &gt;= '2017-03-01' AND C.create_time &lt;= '2017-04-30 23:59:59'
			-- AND D.id = 61 -- Fora da Região
			AND D.id = 17 -- Mais mensagem de erro
	ORDER BY C.create_time DESC
) AS SUBQUERY
WHERE	MENSAGEM_ENVIADA = 'Cadastrar CV no Vagas'  ;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
